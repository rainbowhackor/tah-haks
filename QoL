valg=3
chosencrown = nil  crown1 = nil  crownhat = nil crownmesh = nil HatAttachment = nil  hconstraint = nil  FakeFace = nil 
function Crown1()--client sided, no one can see this  
	for _,shad in pairs(char:GetDescendants()) do if shad:IsA("BasePart") then shad.CastShadow = false end end --this fixed the shadow glitch, not quite sure why the vertex color wont glow on mobile
	hedas = char.Humanoid.HeadScale.Value 
	if workspace:FindFirstChild("GlowyHats") then workspace:FindFirstChild("GlowyHats"):Remove() return end
	if ctext == 1 then styles = CrownTexture1 end if ctext == 2 then styles = CrownTexture2 end
	chosencrown = styles[math.random(1, #styles)] 
	crown1 = Instance.new("Folder",workspace) crown1.Name = "GlowyHats"
	crownhat = Instance.new("Part",crown1) crownhat.Name = "GlowyHat" crownhat.CanCollide = false crownhat.Massless = true crownhat.CanQuery = false
	crownmesh = Instance.new("SpecialMesh",crownhat) crownmesh.MeshId = "http://www.roblox.com/asset/?id=88878197" crownmesh.TextureId = chosencrown 
	--glowy = math.random(1,5)   if glowy == 2 then crowncolor.VertexColor = Vector3.new(1.5, 1.5, 1.5) end if glowy == 3 then crowncolor.VertexColor = Vector3.new(2, 2, 2) end if glowy == 4 then crowncolor.VertexColor = Vector3.new(3, 3, 3) end if glowy == 5 then crowncolor.VertexColor = Vector3.new(4, 4, 4) end
	crownmesh.VertexColor = Vector3.new(valg/2, valg/2, valg/2) crownmesh.Scale = Vector3.new(1*hedas, 1*hedas, 1*hedas)
	HatAttachment = Instance.new("Attachment",crownhat) HatAttachment.Name = "HatAttachment" HatAttachment.Position = Vector3.new(0, -0.285*hedas, 0.047*hedas)
	hconstraint = Instance.new("RigidConstraint",crownhat) hconstraint.Attachment0 = HatAttachment hconstraint.Attachment1 = char.Head.HatAttachment
end

function Crown2()--client sided, no one can see this  
	for _,shad in pairs(char:GetDescendants()) do if shad:IsA("BasePart") then shad.CastShadow = false end end  
	hedas = char.Humanoid.HeadScale.Value 
	if workspace:FindFirstChild("GlowyHats") then workspace:FindFirstChild("GlowyHats"):Remove() return end
	if ctext == 1 then styles = CrownTexture1 end if ctext == 2 then styles = CrownTexture2 end
	chosencrown = styles[math.random(1, #styles)] 
	crown1 = Instance.new("Folder",workspace) crown1.Name = "GlowyHats" 
	crownhat = Instance.new("Part",crown1) crownhat.Name = "GlowyHat" crownhat.CanCollide = false crownhat.Massless = true crownhat.CanQuery = false
	crownmesh = Instance.new("SpecialMesh",crownhat) crownmesh.MeshId = "http://www.roblox.com/asset/?id=187938428" crownmesh.TextureId = chosencrown
	--glowy = math.random(1,5)   if glowy == 2 then crowncolor.VertexColor = Vector3.new(1.5, 1.5, 1.5) end if glowy == 3 then crowncolor.VertexColor = Vector3.new(2, 2, 2) end if glowy == 4 then crowncolor.VertexColor = Vector3.new(3, 3, 3) end if glowy == 5 then crowncolor.VertexColor = Vector3.new(4, 4, 4) end
	crownmesh.VertexColor = Vector3.new(valg/2, valg/2, valg/2) crownmesh.Scale = Vector3.new(1*hedas, 1*hedas, 1*hedas)
	HatAttachment = Instance.new("Attachment",crownhat) HatAttachment.Name = "HatAttachment" HatAttachment.Position = Vector3.new(0, -0.285*hedas, -0)
	hconstraint = Instance.new("RigidConstraint",crownhat) hconstraint.Attachment0 = HatAttachment hconstraint.Attachment1 = char.Head.HatAttachment
end

function reglow1()--client sided, no one can see this  
	for _,t in pairs(workspace.GlowyHats:GetDescendants()) do if t.Name == "GlowyHat" then t.Mesh.VertexColor = Vector3.new(valg/2, valg/2, valg/2) end end
end

function NewHead1() if char:FindFirstChild("FakeHead") then char:FindFirstChild("FakeHead"):Remove() char.Head.Transparency = 0 for _,shad in pairs(char.Head.OriginalSize:GetDescendants()) do shad.Parent = char.Head end return end
	FakeHead = Instance.new("Part",char) FakeHead.Name = "FakeHead" FakeHead.Size = char.Head.Size 
	FakeHead.CanCollide = false FakeHead.Massless = true FakeHead.CanQuery = false FakeHead.Color = char.Head.Color char.Head.Transparency = 1
	HatMesh = Instance.new("SpecialMesh",FakeHead) hweld = Instance.new("Weld",FakeHead) hweld.Part0=FakeHead hweld.Part1=char.Head 
	FakeFace = Instance.new("Decal",FakeHead) FakeFace.Face = Enum.NormalId.Front FakeFace.Name = "face" 
	for _,shad in pairs(char.Head:GetChildren()) do if shad:IsA("Decal") then shad.Parent = char.Head.OriginalSize end end 
end

ThisIs = {" Character Effects "} --no one can see these effects  
function OriginalHealLight() if char.UpperTorso:FindFirstChild("HealLight0") then char.UpperTorso.HealLight0:Remove() return end Original_HealLight:Clone().Parent = char.UpperTorso end
function stardust() if char.UpperTorso:FindFirstChild("Starz") then char.UpperTorso.Starz:Remove() return end starz:Clone().Parent = char.UpperTorso end
function heallight() if char.UpperTorso:FindFirstChild("HealLight2") then char.UpperTorso.HealLight2:Remove() return end HealLight:Clone().Parent = char.UpperTorso end
function potionlight() if char.UpperTorso:FindFirstChild("HealLight") then char.UpperTorso.HealLight:Remove() return end Potionlight:Clone().Parent = char.UpperTorso end
function katanasparkle1()
	if char.UpperTorso:FindFirstChild("Sparkles1") then char.UpperTorso.Sparkles1:Remove() char.UpperTorso.Sparkles2:Remove() return end
	local Sparkles1 = Instance.new("Sparkles")  Sparkles1.SparkleColor = Color3.fromRGB(-765, -1020, 1530) Sparkles1.Name = "Sparkles1" Sparkles1.Parent = char.UpperTorso 
	local Sparkles2 = Instance.new("Sparkles")  Sparkles2.SparkleColor = Color3.fromRGB(1020, -255, 1275) Sparkles2.Name = "Sparkles2" Sparkles2.Parent = char.UpperTorso 
	repeat  task.wait(3 ) 
		Sparkles1.SparkleColor = Color3.new(math.random(0, 5), math.random(0, 12), math.random(0, 15)) --kLight1.Color = Sparkles1.Color
		Sparkles2.SparkleColor = Color3.new(math.random(0, 12), math.random(0, 3), math.random(0, 15))
	until not Sparkles1  
end

function SparkleSet() for _,sp in pairs(char.UpperTorso:GetChildren()) do if sp.Name =="SparkleSet" or sp.Name =="RainbowSparkles" then sp:Remove() end end 
	if ColorSet == "Murica" then spark = Instance.new("Sparkles",char.UpperTorso) spark.SparkleColor = Color3.fromRGB(765, 0, 0) spark.Name = "SparkleSet" spark = Instance.new("Sparkles",char.UpperTorso) spark.SparkleColor = Color3.fromRGB(0, 0, 765) spark.Name = "SparkleSet" end
	if ColorSet == "Murica2" then spark = Instance.new("Sparkles",char.UpperTorso) spark.SparkleColor = Color3.fromRGB(765, 0, 0) spark.Name = "SparkleSet" spark = Instance.new("Sparkles",char.UpperTorso) spark.SparkleColor = Color3.fromRGB(0, 0, 765) spark.Name = "SparkleSet" spark = Instance.new("Sparkles",char.UpperTorso) spark.SparkleColor = Color3.fromRGB(765, 765, 765) spark.Name = "SparkleSet" end
	if ColorSet == "Leprechaun" then spark = Instance.new("Sparkles",char.UpperTorso) spark.SparkleColor = Color3.fromRGB(255, 170, 0) spark.Name = "SparkleSet" spark = Instance.new("Sparkles",char.UpperTorso) spark.SparkleColor = Color3.fromRGB(0, 170, 0) spark.Name = "SparkleSet" end
	if ColorSet == "Joka" then spark = Instance.new("Sparkles",char.UpperTorso) spark.SparkleColor = Color3.fromRGB(144, 25, 255) spark.Name = "SparkleSet" spark = Instance.new("Sparkles",char.UpperTorso) spark.SparkleColor = Color3.fromRGB(1, 0, 3) spark.Name = "SparkleSet" spark = Instance.new("Sparkles",char.UpperTorso) spark.SparkleColor = Color3.fromRGB(0, 170, 0) spark.Name = "SparkleSet" end
	if ColorSet == "Checkers" then spark = Instance.new("Sparkles",char.UpperTorso) spark.SparkleColor = Color3.fromRGB(225, 0, 0) spark.Name = "SparkleSet" spark = Instance.new("Sparkles",char.UpperTorso) spark.SparkleColor = Color3.fromRGB(1, 0, 3) spark.Name = "SparkleSet" end
	if ColorSet == "No Sparkle" then end
	--76, 184, 255
end

