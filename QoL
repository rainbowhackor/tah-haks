function AnimSet()
	for _,anims in pairs(char.Animate.idle:GetChildren()) do if a2:IsA("Animation") and a2.Name ~= "Animation1" then a2.Name = "Animation2" end end 

	if not char.Animate.idle:FindFirstChild("Animation1") then local a1 = char.Animate.idle:FindFirstChildWhichIsA("Animation") a1.Name = "Animation1" 
		for _,a2 in pairs(char.Animate.idle:GetChildren()) do if a2:IsA("Animation") and a2.Name ~= "Animation1" then a2.Name = "Animation2" end end 
	end
	for _,a2 in pairs(char.Animate.climb:GetChildren()) do if a2:IsA("Animation") then a2.Name = "ClimbAnim" end end
	for _,a2 in pairs(char.Animate.fall:GetChildren()) do if a2:IsA("Animation") then a2.Name = "FallAnim" end end
	for _,a2 in pairs(char.Animate.jump:GetChildren()) do if a2:IsA("Animation") then a2.Name = "JumpAnim" end end
	for _,a2 in pairs(char.Animate.run:GetChildren()) do if a2:IsA("Animation") then a2.Name = "RunAnim" end end
	if char.Animate:FindFirstChild("swim") then for _,a2 in pairs(char.Animate.swim:GetChildren()) do if a2:IsA("Animation") then a2.Name = "Swim" end end
		for _,a2 in pairs(char.Animate.swimidle:GetChildren()) do if a2:IsA("Animation") then a2.Name = "SwimIdle" end end end
	for _,a2 in pairs(char.Animate.walk:GetChildren()) do if a2:IsA("Animation") then a2.Name = "WalkAnim" end end
	--for _,a2 in pairs(char.Animate.idle:GetChildren()) do if a2:IsA("Animation") then a2.Name = "Animation2" end end
	--for _,a2 in pairs(char.Animate.idle:GetChildren()) do if a2:IsA("Animation") then a2.Name = "Animation2" end end
	if not char.Animate.idle:FindFirstChild("Animation2") then local a1 = char.Animate.idle:FindFirstChildWhichIsA("Animation"):Clone() a1.Parent = char.Animate.idle a1.Name = "Animation2"  end

	anim = char.Animate
	fall = anim.fall.FallAnim
	idle1 = anim.idle.Animation1 
	idle2 = anim.idle.Animation2 
	jump = anim.jump.JumpAnim
	run = anim.run.RunAnim
	if anim:FindFirstChild("swim") then swim = anim.swim.Swim 
		swimidle = anim.swimidle.SwimIdle end
	walk = anim.walk.WalkAnim
	sit = anim.sit.SitAnim
	climb = anim.climb.ClimbAnim

	char.Humanoid.PlatformStand = true wait(.5)--.2

	if an == 0 or an == "0" then  ThisIs = {"goofy ah"}
		idle1.Weight.Value = "6"					idle2.Weight.Value = "4"
		idle1.AnimationId = WolfSwimIdle 		idle2.AnimationId = CartoonSwimIdle --astroidle  idle2.AnimationId =  "http://www.roblox.com/asset/?id=891621366"
		jump.AnimationId = StyleJump 			fall.AnimationId = adiniball1	
		swim.AnimationId = WolfSwim			 	swimidle.AnimationId = WolfSwimIdle 
		run.AnimationId = Confident_Walk 		walk.AnimationId = Ninja_walk
		sit.AnimationId = WolfSwimIdle
		char.Animate.toolnone:FindFirstChild("ToolNoneAnim").AnimationId = "rbxassetid://14366558676"
	end
	if an == 1 then  ThisIs = {"robo swim"}
		idle1.Weight.Value = "6"					idle2.Weight.Value = "4"
		swim.AnimationId = RobotSwim 			swimidle.AnimationId = RobotSwimIdle
		idle1.AnimationId = RobotSwimIdle 		idle2.AnimationId = RobotSwim
		jump.AnimationId = StyleJump 			fall.AnimationId = StyleFall	
		run.AnimationId = Confident_Walk 		walk.AnimationId = Ninja_walk
		sit.AnimationId = adiniball1 
	end
	if an == 2 then  ThisIs = {"wolf swim"}
		idle1.Weight.Value = "6"					idle2.Weight.Value = "4"
		idle1.AnimationId = WolfSwimIdle  		idle2.AnimationId = CartoonSwimIdle
		jump.AnimationId = NormJump 				fall.AnimationId = WolfFall
		swim.AnimationId = WolfSwim			 	swimidle.AnimationId = WolfSwimIdle 
		run.AnimationId = WolfFall				walk.AnimationId = Confident_Walk 
		sit.AnimationId = WolfSwimIdle 
	end
	if an == 4 then  ThisIs = {"me pop"}
		idle1.AnimationId = PopIdle 				idle2.AnimationId = PopIdle
		jump.AnimationId = StyleJump 			fall.AnimationId = StyleFall
		run.AnimationId = Confident_Walk 		walk.AnimationId = Bubble_Walk
		sit.AnimationId = WolfSwimIdle 
	end
	if an == 44 then  ThisIs = {"me1 "}
		idle1.AnimationId = "http://www.roblox.com/asset/?id=10921132962" idle2.AnimationId = "http://www.roblox.com/asset/?id=10921133721" 
		jump.AnimationId = StyleJump 			fall.AnimationId = StyleFall										
		swim.AnimationId = RobotSwim 			swimidle.AnimationId = RobotSwimIdle
		run.AnimationId = "http://www.roblox.com/asset/?id=656118852"	walk.AnimationId = Bubble_Walk
	end

	if an == 5 then ThisIs = {"norm"}
		idle1.AnimationId = barock_idle			idle2.AnimationId = barock_idle
		jump.AnimationId = NormJump 				fall.AnimationId = NormFall									
		swim.AnimationId = NormSwim			 	swimidle.AnimationId = NormSwimIdle
		run.AnimationId = NormRun 				walk.AnimationId = Pirate_Walk
	end
	if an == 169402 then  ThisIs = {"me"}
		idle1.Weight.Value = "6" idle2.Weight.Value = "4" idle1.AnimationId = WolfSwimIdle idle2.AnimationId = CartoonSwimIdle 
		swim.AnimationId = WolfSwim	swimidle.AnimationId = WolfSwimIdle sit.AnimationId = WolfSwimIdle run.AnimationId = Confident_Walk
		char.Animate.toolnone:FindFirstChild("ToolNoneAnim").AnimationId = "rbxassetid://14366558676"
		--naniwalk = math.random(1,4) if naniwalk ~= 1 then run.AnimationId = Confident_Walk else run.AnimationId = WolfFall REMOVE_LEGS() end -- "http://www.roblox.com/asset/?id=656118852" end
	end
	if an == 6 then  ThisIs = {"adini"}
		idle1.Weight.Value = "6" idle2.Weight.Value = "4" idle1.AnimationId = WolfSwimIdle idle2.AnimationId = CartoonSwimIdle 
		swim.AnimationId = adiniswim	swimidle.AnimationId = WolfSwimIdle sit.AnimationId = WolfSwimIdle run.AnimationId = Confident_Walk
		char.Animate.toolnone:FindFirstChild("ToolNoneAnim").AnimationId = "rbxassetid://14366558676"
		--naniwalk = math.random(1,4) if naniwalk ~= 1 then run.AnimationId = Confident_Walk else run.AnimationId = WolfFall REMOVE_LEGS() end -- "http://www.roblox.com/asset/?id=656118852" end
	end 
	if an == 3 then  ThisIs = {"adini ball"}
		--idle1.AnimationId = adiniball1 idle2.AnimationId = adiniball2
		idle1.Weight.Value = "4"					idle2.Weight.Value = "6"
		idle1.AnimationId = WolfSwimIdle 		idle2.AnimationId = CartoonSwimIdle
		swim.AnimationId = adiniswim	swimidle.AnimationId = WolfSwimIdle sit.AnimationId = adiniball1 
		char.Animate.toolnone:FindFirstChild("ToolNoneAnim").AnimationId = "rbxassetid://14366558676"
		run.AnimationId = adiniball1 				walk.AnimationId = adiniball2
		jump.AnimationId = adiniball1 				fall.AnimationId = adiniball2
		--naniwalk = math.random(1,4) if naniwalk ~= 1 then run.AnimationId = Confident_Walk else run.AnimationId = WolfFall REMOVE_LEGS() end -- "http://www.roblox.com/asset/?id=656118852" end
	end

	wait(.5) char.Humanoid.PlatformStand = false --.1
	an = an + 1 if an >= 5 then an = 0 end
end

function AnimRun()
	for _,a2 in pairs(char.Animate.run:GetChildren()) do if a2:IsA("Animation") then a2.Name = "RunAnim" end end 
	for _,a2 in pairs(char.Animate.walk:GetChildren()) do if a2:IsA("Animation") then a2.Name = "WalkAnim" end end 
	run = char.Animate.run.RunAnim walk = char.Animate.walk.WalkAnim

	char.Humanoid.PlatformStand = true wait(.5) --.2
	if runan == 0 then  ThisIs = {"Confident_Walk"} run.AnimationId = Confident_Walk walk.AnimationId = Ninja_walk end
	if runan == 1 then  ThisIs = {"goofy goober run"} run.AnimationId = "http://www.roblox.com/asset/?id=16738337225" walk.AnimationId = "http://www.roblox.com/asset/?id=16738340646" end
	if runan == 2 then  ThisIs = {"goofy goober walk"} run.AnimationId = "http://www.roblox.com/asset/?id=16738340646" walk.AnimationId = "http://www.roblox.com/asset/?id=16738337225" end
	if runan == 3 then  ThisIs = {"ninja walkrun"}  run.AnimationId = Ninja_walk  walk.AnimationId = Confident_Walk end
	if runan == 4 then  ThisIs = {"ninja run"} run.AnimationId = "http://www.roblox.com/asset/?id=656118852" end
	if runan == 5 then  ThisIs = {"wolf fall"} run.AnimationId = WolfFall end
	if runan == 14 then  ThisIs = {"heavy run"} run.AnimationId = "http://www.roblox.com/asset/?id=3236836670" end
	if runan == 6 then  ThisIs = {"adiniball"} run.AnimationId = adiniball1 end
	if runan == 7 then  ThisIs = {"norm run"} run.AnimationId = NormRun end
	if runan == 8 then  ThisIs = {"supa suik"} run.AnimationId = "http://www.roblox.com/asset/?id=10921291831" end
	if runan == 9 then  ThisIs = {"bubble walk"} run.AnimationId = Bubble_Walk end
	if runan == 10 then  ThisIs = {"zombie run"} run.AnimationId = "http://www.roblox.com/asset/?id=616163682" end
	if runan == 11 then  ThisIs = {"wolf run"} run.AnimationId = "http://www.roblox.com/asset/?id=1083216690" end
	if runan == 12 then  ThisIs = {"hover"} run.AnimationId = NormFall  end
	if runan == 13 then  ThisIs = {"norm swim"} run.AnimationId = "http://www.roblox.com/asset/?id=913384386" end

	wait(.5) char.Humanoid.PlatformStand = false --.1
	runan = runan + 1 if runan >= 13 then runan = 0 end
end

function AnimIdle()
	if not char.Animate.idle:FindFirstChild("Animation1") then local a1 = char.Animate.idle:FindFirstChildWhichIsA("Animation") a1.Name = "Animation1" 
		for _,a2 in pairs(char.Animate.idle:GetChildren()) do if a2:IsA("Animation") and a2.Name ~= "Animation1" then a2.Name = "Animation2" end end 
	end
	if not char.Animate.idle:FindFirstChild("Animation2") then local a1 = char.Animate.idle:FindFirstChildWhichIsA("Animation"):Clone() a1.Parent = char.Animate.idle a1.Name = "Animation2"  end
	idle1 = char.Animate.idle.Animation1 idle2 = char.Animate.idle.Animation2 
	char.Humanoid.PlatformStand = true task.wait(.5)--.2

	ThisIs = {"stand"}
	if an == 1 then idle1.Weight.Value = "6" idle2.Weight.Value = "4" idle1.AnimationId = WolfSwimIdle idle2.AnimationId = CartoonSwimIdle end ThisIs = {"swimwolf"}
	if an == 2 then idle1.Weight.Value = "7" idle2.Weight.Value = "3" idle1.AnimationId = RobotSwimIdle idle2.AnimationId = RobotSwim end ThisIs = {"swimrobo"} 

	if an == 3 then  idle1.AnimationId = PopIdle idle2.AnimationId = idle1.AnimationId end ThisIs = {"pop"} 
	if an == 4 then  idle1.AnimationId = "http://www.roblox.com/asset/?id=16738333868" idle2.AnimationId = "http://www.roblox.com/asset/?id=16738334710"end ThisIs = {"gorly"}--barock_idle idle2.AnimationId = idle1.AnimationId end 

	if an == 5 then  idle1.AnimationId = "http://www.roblox.com/asset/?id=17172918855" 	idle2.AnimationId = "http://www.roblox.com/asset/?id=17173014241" end ThisIs = {"real"} 
	if an == 6 then  idle1.AnimationId = "http://www.roblox.com/asset/?id=10921230744"	idle2.AnimationId =  idle1.AnimationId end ThisIs = {"rthro"} 

	if an == 7 then  idle1.AnimationId = "http://www.roblox.com/asset/?id=10921258489"	idle2.AnimationId = idle1.AnimationId end ThisIs = {"barock"} 
	if an == 8 then  idle1.AnimationId = "http://www.roblox.com/asset/?id=10921230744"	idle2.AnimationId =  idle1.AnimationId end ThisIs = {"oldsch"} 
	if an == 9 then  idle1.AnimationId = adiniball1 idle2.AnimationId = adiniball2 end ThisIs = {"adini"} 
	if an == 18 then  idle1.AnimationId = "http://www.roblox.com/asset/?id=10921132962" idle2.AnimationId = "http://www.roblox.com/asset/?id=10921133721" end ThisIs = {"levitate"} 
	if an == 10 then  idle1.AnimationId = Wolf_idle idle2.AnimationId = idle1.AnimationId end ThisIs = {"wolf"} 
	if an == 11 then  idle1.AnimationId = "http://www.roblox.com/asset/?id=742637544" idle2.AnimationId = idle1.AnimationId end  ThisIs = {"cartoon"} 
	if an == 12 then idle1.AnimationId = "http://www.roblox.com/asset/?id=891621366" idle2.AnimationId = idle1.AnimationId end  ThisIs = {"astro"} 
	if an == 13 then idle1.AnimationId = "http://www.roblox.com/asset/?id=4417977954" idle2.AnimationId = idle1.AnimationId end  ThisIs = {"toilet"} 
	if an == 14 then idle1.AnimationId = "http://www.roblox.com/asset/?id=656117400" idle2.AnimationId = idle1.AnimationId end  ThisIs = {"ninja"} 
	if an == 15 then idle1.AnimationId = "http://www.roblox.com/asset/?id=10921101664" idle2.AnimationId = idle1.AnimationId end  ThisIs = {"elder"} 
	if an == 16 then idle1.AnimationId = "http://www.roblox.com/asset/?id=10921117521" idle2.AnimationId = idle1.AnimationId end  ThisIs = {"knight"}
	if an == 17 then idle1.AnimationId = "http://www.roblox.com/asset/?id=10921301576" idle2.AnimationId = idle1.AnimationId end  ThisIs = {"toy"} 
	--if an == 18 then idle1.AnimationId = "http://www.roblox.com/asset/?id=16738333868" idle2.AnimationId = "http://www.roblox.com/asset/?id=16738334710" end  ThisIs = {"gorly"} 
	wait(.5) char.Humanoid.PlatformStand = false --.1
end
toolref = nil  
function AnimOther() 
	anim = char.Animate fall = anim.fall.FallAnim  jump = anim.jump.JumpAnim 
	if anim:FindFirstChild("swim") then swim = anim.swim.Swim 
		swimidle = anim.swimidle.SwimIdle end
	sit = anim.sit.SitAnim climb = anim.climb.ClimbAnim
	char.Humanoid.PlatformStand = true wait(.5)--.2
	ThisIs = {"jump"}
	if an == 99 then  ThisIs = {"No Tool Anim"} if char:FindFirstChild("Animate") and char.Animate:FindFirstChild("toolnone") and char.Animate.toolnone:FindFirstChild("ToolNoneAnim") then char.Animate.toolnone:FindFirstChild("ToolNoneAnim").AnimationId = "rbxassetid://14366558676" if char:FindFirstChildWhichIsA("Tool") then toolref=char:FindFirstChildWhichIsA("Tool") toolref.Parent = player.Backpack wait(.5) toolref.Parent = char end end end --Mood Animation
	if an == -1000 then  ThisIs = {"Tool Anim"} if char:FindFirstChild("Animate") and char.Animate:FindFirstChild("toolnone") and char.Animate.toolnone:FindFirstChild("ToolNoneAnim") then char.Animate.toolnone:FindFirstChild("ToolNoneAnim").AnimationId = "http://www.roblox.com/asset/?id=507768375" if char:FindFirstChildWhichIsA("Tool") then toolref=char:FindFirstChildWhichIsA("Tool") toolref.Parent = player.Backpack wait(.5) toolref.Parent = char end end end 
	if an == 0 then  ThisIs = {"rthro jump"} jump.AnimationId = NormJump 	fall.AnimationId = NormFall end
	if an == 1 then  ThisIs = {"StyleJump"} jump.AnimationId = StyleJump 	fall.AnimationId = StyleFall end
	if an == 2 then  ThisIs = {"WolfFall"} fall.AnimationId = WolfFall end
	if an == 3 then  ThisIs = {"adini"} jump.AnimationId = adiniball1 	fall.AnimationId = adiniball2  end

	ThisIs = {"climb"}
	if an == 4 then  ThisIs = {"rthro"} climb.AnimationId = "http://www.roblox.com/asset/?id=656114359" end
	if an == 5 then  ThisIs = {"wolf"} climb.AnimationId = "http://www.roblox.com/asset/?id=10921329322" end
	if an == 6 then  ThisIs = {"astro"} climb.AnimationId = "http://www.roblox.com/asset/?id=891609353" end
	if an == 7 then  	  						  end

	ThisIs = {"sit"}
	if an == 8 then  ThisIs = {"rthro"} sit.AnimationId = "http://www.roblox.com/asset/?id=2506281703" end
	if an == 9 then  ThisIs = {"wolf"} sit.AnimationId = WolfSwimIdle end
	if an == 10 then  ThisIs = {"cartoon"} sit.AnimationId = CartoonSwimIdle end
	if an == 11 then  ThisIs = {"robo swim"} sit.AnimationId = RobotSwimIdle end
	if an == 11.2 then  ThisIs = {"adiniball"} sit.AnimationId = adiniball1 end

	ThisIs = {"swim"}
	if an == 12 then  ThisIs = {"rthro"} swim.AnimationId = NormSwim	 swimidle.AnimationId = NormSwimIdle end
	if an == 13 then  ThisIs = {"wolf"} swim.AnimationId = WolfSwim	swimidle.AnimationId = WolfSwimIdle end
	if an == 14 then  ThisIs = {"robo"} swim.AnimationId = RobotSwim swimidle.AnimationId = RobotSwimIdle end
	if an == 15 then  ThisIs = {"adini"} swim.AnimationId = adiniswim swimidle.AnimationId = adiniball1 end
	wait(.5) char.Humanoid.PlatformStand = false --.1
end





