ThisIs = {"Clickable Gui", }  
player.CharacterRemoving:Connect(function() if player.PlayerGui:FindFirstChild("MAMALUIGI") then player.PlayerGui:FindFirstChild("MAMALUIGI"):Remove() end end)
function guias() 
	if player.Character.Humanoid.Health < 1 then return end 
	if player.PlayerGui:FindFirstChild("MAMALUIGI") then return end 

	--if player.PlayerGui:FindFirstChild("MAMALUIGI") then player.PlayerGui:FindFirstChild("MAMALUIGI"):Remove() end
	if not player.PlayerGui:FindFirstChild("MAMALUIGI") then 
		--if not game.StarterGui:FindFirstChild("MAMALUIGI") then 
		GuiLuigi = Instance.new("ScreenGui",player.PlayerGui) GuiLuigi.Name = "MAMALUIGI" GuiLuigi.Enabled = true

		Button = Instance.new("TextButton") Button.Name = "Button"  Button.Position = UDim2.new(0, -0 , 0, 0) Button.Size = UDim2.new(0, 60 , 0, 15)
		Button.BackgroundColor3 = Color3.fromRGB(0, 255, 255) Button.BackgroundTransparency = .6 Button.TextSize = 11 
		Button.TextColor3 = Color3.fromRGB(255, 255, 255)

		Scroll = Instance.new("ScrollingFrame") Scroll.Name = "bingchilin" Scroll.Position = UDim2.new(0.15, 0 , 0.66, 0) Scroll.Size = UDim2.new(0.1, 0 , 0.0, 190) Scroll.BackgroundColor3 = Color3.fromRGB(0, 0, 0) Scroll.BackgroundTransparency = .7 Scroll.CanvasSize = UDim2.new(0, 0 , 2, 0) Scroll.ScrollBarThickness = 4 
		notScroll = Instance.new("Frame") notScroll.Name = "access" notScroll.Position = UDim2.new(0.15, 0 , 0.3, 0)  notScroll.Size = UDim2.new(0.1, 0 , 0.0, 160) notScroll.BackgroundColor3 = Color3.fromRGB(0, 0, 0) notScroll.BackgroundTransparency = 1 

		TextBox = Instance.new("TextBox") TextBox.Name = "TextBox"  TextBox.Position = UDim2.new(0, -0 , 0, 0) TextBox.Size = UDim2.new(0, 60 , 0, 15)
		TextBox.BackgroundColor3 = Color3.fromRGB(0, 255, 255) TextBox.BackgroundTransparency = .6 TextBox.TextSize = 11
		TextBox.TextColor3 = Color3.fromRGB(255, 255, 255) TextBox.Text = "" TextBox.PlaceholderColor3 = Color3.fromRGB(255, 255, 255)
		FaceButton = Instance.new("ImageButton") FaceButton.BackgroundTransparency = .8 FaceButton.Size = UDim2.new(0.33,0 , 0.33,0)

		Begal = Button:Clone() Begal.Parent = GuiLuigi Begal.Text = "Hax gui" Begal.Position = UDim2.new(0.25, 0 , 0.85, 0) Begal.BackgroundTransparency = .6  Begal.BackgroundColor3 = Color3.fromRGB(255, 85, 0)
		task.spawn(function() task.wait(10) Begal.TextColor3 = Color3.fromRGB(170, 170, 255) Begal.BackgroundColor3 = Color3.fromRGB(170, 85, 0) end)--27, 42, 53
		GuiLuigi2 = Instance.new("ScreenGui",Begal) GuiLuigi2.Name = "MAMALUIGI2"  GuiLuigi2.Enabled = false 
		Spaghetti = Scroll:Clone() Spaghetti.Parent = GuiLuigi2 Spaghetti.CanvasSize = UDim2.new(0, 0 , .9, 0)

		Begal.MouseButton1Click:Connect(function() 	
			GuiLuigi2.Enabled = not GuiLuigi2.Enabled
			if GuiLuigi2.Enabled == true then Begal.TextColor3 = Color3.fromRGB(255, 255, 255) Begal.BackgroundColor3 = Color3.fromRGB(255, 85, 0) else 
				Begal.TextColor3 = Color3.fromRGB(27, 42, 53) Begal.BackgroundColor3 = Color3.fromRGB(170, 85, 0)  
				for _,alb in pairs(GuiLuigi:GetDescendants()) do if alb:IsA("ScreenGui") then alb.Enabled = false end end
			end
		end)
		Begal.MouseButton2Click:Connect(function() 	
			Begal:FindFirstChild("TIME TO DIY 3").Enabled = not Begal:FindFirstChild("TIME TO DIY 3").Enabled
			if Begal:FindFirstChild("TIME TO DIY 3").Enabled == true then 
				Begal.TextColor3 = Color3.fromRGB(255, 255, 255) Begal.BackgroundColor3 = Color3.fromRGB(255, 85, 0) else
				Begal.TextColor3 = Color3.fromRGB(27, 42, 53) Begal.BackgroundColor3 = Color3.fromRGB(170, 85, 0) 
			end
		end)
		Begal.MouseWheelForward:Connect(function() Begal.TextColor3 = Color3.fromRGB(27, 42, 53) Begal.BackgroundColor3 = Color3.fromRGB(170, 85, 0) end)
		Begal.MouseWheelBackward:Connect(function() Begal.TextColor3 = Color3.fromRGB(255, 255, 255) Begal.BackgroundColor3 = Color3.fromRGB(255, 85, 0) end)

