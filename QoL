task.wait(1)
guias()

if not game.Players.LocalPlayer:FindFirstChild("logs1") then log1 = Instance.new("Folder",game.Players.LocalPlayer) log1.Name = "logs1" end
for _, person in pairs(game.Players:GetChildren()) do if person:IsA("Player") then --bro actually put a spawnlocation in players and broke this
		local whoarethey = Instance.new("Folder",game.Players.LocalPlayer.logs1)
		realname = Instance.new("StringValue",whoarethey) realname.Name = "realname" realname.Value = person.Name
		nickname = Instance.new("StringValue",whoarethey) nickname.Name = "nickname" nickname.Value = person.DisplayName
		pchar = Instance.new("NumberValue",whoarethey) pchar.Name = "id" pchar.Value = person.CharacterAppearanceId 
		imageval = Instance.new("Decal",whoarethey)  imageval.Name = "image" imageval.Texture = game.Players:GetUserThumbnailAsync(person.CharacterAppearanceId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
		imageval2 = Instance.new("Decal",whoarethey)  imageval2.Name = "image2" imageval2.Texture = game.Players:GetUserThumbnailAsync(person.CharacterAppearanceId, Enum.ThumbnailType.AvatarThumbnail, Enum.ThumbnailSize.Size420x420)
		task.spawn(function() task.wait(1800) whoarethey:Remove() end) end
end
game.Players.PlayerAdded:Connect(function(person) task.wait(.1) if game.Players.LocalPlayer.logs1:FindFirstChild(person.UserId) then return end
	local whoarethey = Instance.new("Folder",game.Players.LocalPlayer.logs1) whoarethey.Name = person.UserId
	local realname = Instance.new("StringValue",whoarethey) realname.Name = "realname" realname.Value = person.Name
	local nickname = Instance.new("StringValue",whoarethey) nickname.Name = "nickname" nickname.Value = person.DisplayName
	local pchar = Instance.new("NumberValue",whoarethey) pchar.Name = "id" pchar.Value = person.CharacterAppearanceId 

	local imageval = Instance.new("Decal",whoarethey)  imageval.Name = "image" imageval.Texture = game.Players:GetUserThumbnailAsync(person.CharacterAppearanceId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
	local imageval2 = Instance.new("Decal",whoarethey)  imageval2.Name = "image2" imageval2.Texture = game.Players:GetUserThumbnailAsync(person.CharacterAppearanceId, Enum.ThumbnailType.AvatarThumbnail, Enum.ThumbnailSize.Size420x420)
	--task.spawn(function() task.wait(600) whoarethey:Remove() end)
	--[[task.spawn(function() task.wait(600) imageval.Texture = game.Players:GetUserThumbnailAsync(pchar.Value , Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
		imageval2.Texture = game.Players:GetUserThumbnailAsync(pchar.Value , Enum.ThumbnailType.AvatarThumbnail, Enum.ThumbnailSize.Size420x420)
	end)]]
end)

if workspace:FindFirstChild("NPC") then 
	workspace:FindFirstChild("NPC"):Remove()
end
if workspace:FindFirstChild("Map") then 
	if workspace.Map:FindFirstChild("Leaderboards") then workspace.Map:FindFirstChild("Leaderboards"):Remove() end
	if workspace.Map:FindFirstChild("Other") then workspace.Map:FindFirstChild("Other"):Remove() end
	if workspace.Map:FindFirstChild("Teleporters") then workspace.Map:FindFirstChild("Teleporters"):Remove() end
	if workspace.Map:FindFirstChild("MainIsland") then workspace.Map:FindFirstChild("MainIsland").Name = "Island" end
	if workspace.Map:FindFirstChild("MainIsland") then workspace.Map:FindFirstChild("MainIsland"):Remove() end -- They accidentally duplicated the map in this version of TTD3
	if workspace.Map:FindFirstChild("Island") then 
		for _,L in pairs( workspace.Map:FindFirstChild("Island"):GetDescendants()) do 
			if L.Name == "SoccerField" then L:Remove() end
			if L.Name == "Meshes/Palm Trees V2_Plane.001" or L.Name == "Meshes/Palm Trees V2_Plane.003" or L.Name == "Meshes/ad_Cylinder" or L.Name == "Meshes/ewe_Cylinder.006" or L.Name == "Meshes/ad_Icosphere.006" or L.Name == "Meshes/e_Plane.001 (1)" or L.Name == "Meshes/e_Icosphere.011"  
				or L.Name == "FireVFX" or L.Name == "Meshes/DA_Cylinder.015" or L.Name == "Meshes/DA_Cylinder.017"  or L.Name == "Meshes/DA_Plane"  or L.Name == "Cylinder.003" or L.Name =="Meshes/stone head"
				then L.CanCollide = false
			end
			if L:IsA("BasePart") then if L.Color == Color3.fromRGB(99, 79, 63) then 
				L.CanCollide = false end
			end
			if L:IsA("MeshPart") then 
				if L.MeshId == "rbxassetid://17814036182" or L.MeshId == "rbxassetid://17814036882" or L.MeshId == "rbxassetid://17814036743" or L.MeshId == "rbxassetid://17814036743" then 
					L.CanCollide = false end
				end
		end
	end  
	if workspace.Map:FindFirstChild("Zones") then 
		for _,L in pairs( workspace.Map:FindFirstChild("Zones"):GetChildren()) do 
			if L:IsA("BasePart") then  
					L.CanQuery = false end
			end
		end
	end
	if workspace.Map:FindFirstChild("Volcano") then 
	for _,L in pairs( workspace.Map:FindFirstChild("Volcano"):GetDescendants()) do 
		if L:IsA("ParticleEmitter") then L:Remove() end
			
		end
		end

ThisIsClientSided = {"Walk on ghost dirt", "Drive on ocean"}
function NDS() 
	if player.PlayerScripts:FindFirstChild("UnderwaterVisionLocalScript") then 
		player.PlayerScripts.UnderwaterVisionLocalScript.Enabled = false
		player.PlayerScripts.UnderwaterVisionLocalScript.WaterAtmosphere.Haze = 0
		player.PlayerScripts.UnderwaterVisionLocalScript.WaterAtmosphere.Density = 0
		--game.Lighting.WaterAtmosphere.Haze = 0
		--game.Lighting.WaterAtmosphere.Density = 0
	end
	--if not workspace:FindFirstChild("WeatherMachine") then return end
	if workspace:FindFirstChild("OceanDrive") then return end
	for _, p in pairs(workspace:GetDescendants()) do   
		if p:IsA("BasePart") then
			--if p.Transparency > .96 and p.CanCollide == false and not p:FindFirstChildWhichIsA("ClickDetector") then p.CanQuery = false end
			if p.BrickColor == BrickColor.new("Reddish brown") or p.BrickColor == BrickColor.new("Dark green") then p.CastShadow = false p.CanCollide = true end
			--if p.BrickColor == BrickColor.new("Reddish brown") then p.CastShadow = false p.CanCollide = true end
			--if p.BrickColor == BrickColor.new("Dark green") then p.CastShadow = false end
			if p.Position == Vector3.new(46.5, 27.5, 112.5) or p.Position == Vector3.new(124.5, 27.5, 0.5) or p.Position == Vector3.new(119.5, 27.5, -56.5)


			then
				--or p.Position == Vector3.new(-149.5, 27.5, 16.5) or p.Position == Vector3.new(-148.5, 27.5, 1.5) --or p.Position == Vector3.new(-115, 27.5, -66.5)  
				noquake = Instance.new("Part",p.Parent) noquake.Anchored = true noquake.Color = p.Color 
				noquake.Position = Vector3.new(p.Position.X,p.Position.Y-11.05,p.Position.Z) noquake.Size = Vector3.new(33,p.Size.Y,33) --noquake.BrickColor = BrickColor.new("Brown")
				ghostblock = p:Clone() ghostblock.Parent = p.Parent ghostblock.Transparency = .85 ghostblock.CanCollide = false ghostblock.CastShadow = false ghostblock.CanQuery = false
				if ghostblock.Position == Vector3.new(124.5, 27.5, 0.5) then ghostblock.Position = Vector3.new(129.5, 22.5, 0.5) ghostblock.Size = Vector3.new(1, 1, 15) ghostblock.Transparency = 0 end
				p:Remove() --p.Position = Vector3.new(p.Position.X,p.Position.Y-22,p.Position.Z) --p.Size = Vector3.new(100,p.Size.Y,100) 
			end

			if p.Position == Vector3.new(-149.5, 27.5, 16.5) or p.Position == Vector3.new(-148.5, 27.5, 1.5) 
				or p.Position == Vector3.new(54.5, 27.5, -114)   
			then
				noquake = Instance.new("Part",p.Parent) noquake.Anchored = true noquake.Color = p.Color 
				noquake.Position = Vector3.new(p.Position.X,p.Position.Y-11.05,p.Position.Z) noquake.Size = p.Size noquake.Orientation = p.Orientation --Vector3.new(33,p.Size.Y,33) 
				ghostblock = p:Clone() ghostblock.Parent = p.Parent ghostblock.Transparency = .85 ghostblock.CanCollide = false ghostblock.CastShadow = false ghostblock.CanQuery = false
				p.CanCollide=false --p.Position = Vector3.new(p.Position.X,p.Position.Y-22,p.Position.Z) --p.Size = Vector3.new(100,p.Size.Y,100) 
			end
			if  p.Position == Vector3.new(21.5, 38.5, -70)  --side2 ghost
				or p.Position == Vector3.new(103.5, 38.5, 12.5) --back ghost 
			then
				noquake = Instance.new("Part",p.Parent) noquake.Anchored = true noquake.Color = p.Color 
				noquake.Position = Vector3.new(p.Position.X,p.Position.Y-10.0,p.Position.Z) noquake.Size = p.Size noquake.Orientation = p.Orientation --Vector3.new(33,p.Size.Y,33) 
				ghostblock = p:Clone() ghostblock.Parent = p.Parent ghostblock.Transparency = .85 ghostblock.CanCollide = false ghostblock.CastShadow = false ghostblock.CanQuery = false
				p.Position = Vector3.new(p.Position.X,p.Position.Y+4.5,p.Position.Z) p.Size = Vector3.new(p.Size.X,2,p.Size.Z) 
			end

			p.Locked = false 
			--p.CanTouch = false --ya'know, I genuinely spent 24 hours trying to figure out why my character couldn't touch half the things in my games...
		end 
	end 

	oceanfloors = Instance.new("Part") oceanfloors.Name = "OceanDrive" oceanfloors.BrickColor = BrickColor.new("Sand blue") oceanfloors.Anchored = true 

	if workspace:FindFirstChild("WeatherMachine") then 
		floor = Instance.new("Folder",workspace) floor.Name = "oceanfloors"
		ThisIs = {"drive on ocean floor"} oceanfloor1 = oceanfloors:Clone() oceanfloor1.Parent = floor oceanfloor1.Position = Vector3.new(-30, -27.5, -77) oceanfloor1.Size = Vector3.new(777, 77, 461)  --531) -112
		oceanfloor2 = oceanfloors:Clone() oceanfloor2.Parent = floor oceanfloor2.Position = Vector3.new(-30, -33, 297) oceanfloor2.Size = Vector3.new(777, 66, 287) 
		oceanfloor3 = oceanfloors:Clone() oceanfloor3.Shape = Enum.PartType.Wedge oceanfloor3.Parent = floor oceanfloor3.Orientation = Vector3.new(0, -180, 0) oceanfloor3.Position = Vector3.new(-30, 5.5, 182) oceanfloor3.Size = Vector3.new(777 ,11 ,57)
		oceanfloor4 = oceanfloors:Clone() oceanfloor4.Shape = Enum.PartType.Wedge oceanfloor4.Parent = floor oceanfloor4.Orientation = Vector3.new(0, 0, 0) oceanfloor4.Position = Vector3.new(-30, 5.5, -336) oceanfloor4.Size = Vector3.new(777 ,11 ,57)
		oceanfloor5 = oceanfloors:Clone() oceanfloor5.Parent = floor oceanfloor5.Position = Vector3.new(-30, -33, -451) oceanfloor5.Size = Vector3.new(777, 66, 287) 
		workspace.WaterLevel.CastShadow = false

		UserSettings().GameSettings.ComputerCameraMovementMode = Enum.ComputerCameraMovementMode.CameraToggle
		UserSettings():GetService("UserGameSettings").ComputerCameraMovementMode = Enum.ComputerCameraMovementMode.Classic --HOLY #### THAT ACTUALLY WORKED! (Follow camera is not ideal for mobile emulator)
		UserSettings().GameSettings.ComputerCameraMovementMode = Enum.ComputerCameraMovementMode.Classic
		UserSettings().GameSettings.TouchCameraMovementMode = Enum.TouchCameraMovementMode.Classic 

		--UserSettings().GameSettings.TouchMovementMode = Enum.TouchMovementMode.Default
		--UserSettings().GameSettings.ComputerMovementMode = Enum.ComputerMovementMode.Default
		----UserSettings().GameSettings.ComputerCameraMovementMode = Enum.ComputerCameraMovementMode.Classic
		----UserSettings().GameSettings.ComputerCameraMovementMode = Enum.ComputerCameraMovementMode.CameraToggle
		--UserSettings().GameSettings.TouchCameraMovementMode = Enum.TouchCameraMovementMode.Classic 

		--UserSettings():GetService("UserGameSettings").MasterVolume = .2

		if game.Lighting:FindFirstChildWhichIsA("Atmosphere") then game.Lighting:FindFirstChildWhichIsA("Atmosphere"):Remove() end
		game.Lighting.ChildAdded:Connect(function(ar) wait()   
			if ar:IsA("Atmosphere") then ar:Remove() end
			if game.Lighting:FindFirstChildWhichIsA("Atmosphere") then game.Lighting:FindFirstChildWhichIsA("Atmosphere"):Remove() end
		end)



		--if
		game.Lighting:FindFirstChildWhichIsA("Sky").SkyboxBk = "rbxassetid://15502511288"
		game.Lighting:FindFirstChildWhichIsA("Sky").SkyboxDn = "rbxassetid://15502508460"
		game.Lighting:FindFirstChildWhichIsA("Sky").SkyboxFt = "rbxassetid://15502510289"
		game.Lighting:FindFirstChildWhichIsA("Sky").SkyboxLf = "rbxassetid://15502507918"
		game.Lighting:FindFirstChildWhichIsA("Sky").SkyboxRt = "rbxassetid://15502509398"
		game.Lighting:FindFirstChildWhichIsA("Sky").SkyboxUp = "rbxassetid://15502511911"

	end
	--voidfloor = oceanfloors:Clone() voidfloor.Parent = workspace voidfloor.Position = Vector3.new(-100, -40002, -100) voidfloor.Size = Vector3.new(66, 2, 66)  voidfloor.BrickColor = BrickColor.new("Black")


end

NDS()



if sethiddenproperty then
	--for _,part in pairs(workspace:GetDescendants()) do if part:IsA("BasePart") then part.CanQuery = true end end
	workspace.FallenPartsDestroyHeight = -50000 player.CameraMaxZoomDistance = 1000


	ThisIs = {"invisicam"}
	local sc = (debug and debug.setconstant) or setconstant local gc = (debug and debug.getconstants) or getconstants
	if not sc or not getgc or not gc then return  end --notify = {'this runs on executors but not on default roblox'} 
	local pop = game.Players.LocalPlayer.PlayerScripts.PlayerModule.CameraModule.ZoomController.Popper
	for _, v in pairs(getgc()) do if type(v) == 'function' and getfenv(v).script == pop then
			--for i, v1 in pairs(gc(v)) do if tonumber(v1) == .25 then sc(v, i, 0) elseif tonumber(v1) == 0 then sc(v, i, .25) end end
			for i, v1 in pairs(gc(v)) do if tonumber(v1) == .25 then sc(v, i, 0)   end end
		end
	end
end





