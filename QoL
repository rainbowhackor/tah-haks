				"sturdy","boogie d","breakdow","ramb","samsa","minny","mannr","tuff v","laid b","verv","radic","sou","bubb","shift","freem","introdu", "realit","poplock", " true","hit it","springy","true h","bara b","ken sh","ssy fe","bam b","ss wi","assumpt", "breez","expr","glitt","hype","salsa",}
			for _, faces in pairs(danceName) do local dbutton = Button:Clone() dbutton.Parent = ttdlist dbutton.Text = faces dbutton.Size = UDim2.new(0.95,0 , 0,15) 
				dbutton.MouseButton1Click:Connect(function()
					dbutton.BackgroundColor3 = Color3.fromRGB(0, 170, 0) 
					search = player.PlayerGui.Emotes.MainFrame.Buttons.SearchBar
					--emotes = player.PlayerGui.Emotes.MainFrame.EmoteContainer["1"]
					--emotes.Poplock
					search.Text = "carpet" wait(1)
					search.Text = dbutton.Text   end) end
		end)

		This = "Save sky settings for you game"
		savedL=0
		ligh = Button:Clone() ligh.Parent = Spaghetti ligh.Text = "save Light" ligh.Position = UDim2.new(0.05,0 , 0,row21) ligh.BackgroundColor3 = Color3.fromRGB(255, 170, 0)   
		ligh.MouseButton1Click:Connect(function() 
			gLighting = game.Lighting
			--if game.Lighting:FindFirstChild("savedLighting") then game.Lighting:FindFirstChild("savedLighting"):Remove() end 
			savedL += 1
			local YourSunSet = Instance.new("Configuration",game.Lighting.oof) YourSunSet.Name = "*SavedLighting "..savedL
			if gethiddenproperty then stuoidecompiler = Instance.new("BrickColorValue",YourSunSet) stuoidecompiler.Name = "Lighting" end

			local LightingProperties = {
				["Ambient"] = "Color3",
				["Brightness"] = "Number",
				["ColorShift_Bottom"] = "Color3",
				["ColorShift_Top"] = "Color3",
				["EnvironmentDiffuseScale"] = "Number",
				["EnvironmentSpecularScale"] = "Number",
				["GlobalShadows"] = "Boolean",
				["OutdoorAmbient"] = "Color3",
				["ShadowSoftness"] = "Number",
				["ClockTime"] = "Number",
				["GeographicLatitude"] = "Number",
				["TimeOfDay"] = "String",
				["ExposureCompensation"] = "Number",
				["FogColor"] = "Color3",
				["FogEnd"] = "Number",
				["FogStart"] = "Number"
			}
			local LightingInstances = {
				"Sky",
				"BloomEffect",
				"DepthOfFieldEffect",
				"SunRaysEffect",
				"Atmosphere",
				"BlurEffect",
				"ColorCorrectionEffect"
			}
			local function HasProperty(Inst, Prop)

				--Checks if the property exists in a pcall
				local Success, Value = pcall(function()
					return Inst[Prop]
				end)

				if Success and Value ~= Inst:FindFirstChild(Prop) then --Checks this isn't a children
					return true --Confirms the property does in fact exist
				end
			end
			for Name, Type in pairs(LightingProperties) do
				if HasProperty(gLighting, Name) then

					local Value = nil
					if Type == "Color3" then
						Value = Instance.new("Color3Value")
					elseif Type == "Number" then
						Value = Instance.new("NumberValue")
					elseif Type == "Boolean" then
						Value = Instance.new("BoolValue")
					elseif Type == "String" then
						Value = Instance.new("StringValue")
					else 
						warn("LM: Unexpected type. " .. Type ..".")
					end
					YourSunSet:SetAttribute(Name,gLighting[Name])
					if gethiddenproperty then Value.Value = gLighting[Name] Value.Name = Name Value.Parent = stuoidecompiler end -- oh my gah bro it saved everything exept the Attributes now i gotta rejoin 20 games

				end
			end
			--if gethiddenproperty then YourSunSet:SetAttribute("Technology",gethiddenproperty(game.Lighting, "Technology") end --sethiddenproperty(game.Lighting, "Technology", Enum.Technology.Voxel)
			--if gethiddenproperty then futur = Instance.new("PointLight",YourSunSet) futur.Name = gethiddenproperty(game.Lighting, "Technology") end 
			if gethiddenproperty then futur = Instance.new("Color3Value",YourSunSet) futur.Name =  game.Lighting.Technology.Name end 	

			for Index, Inst in gLighting:GetChildren() do
				local Found = table.find(LightingInstances, Inst.ClassName)
				if Found then --If the instance in lighting is in our table, save it
					local newInst = Inst:Clone()
					newInst.Parent = YourSunSet
				end
			end

			if workspace.Terrain:FindFirstChildWhichIsA("Clouds") then workspace.Terrain:FindFirstChildWhichIsA("Clouds"):Clone().Parent = YourSunSet end
			matval = game.MaterialService:GetChildren() if #matval > 0 then	mats = Instance.new("MaterialVariant",YourSunSet) mats.Name = "Matz" 
				for i,v in pairs(matval) do v:Clone().Parent = mats end end


			Cterrain = Instance.new("TerrainRegion",YourSunSet) Cterrain.Name = "Terrain_Colors"
			Terrain = workspace.Terrain
			Tcolor = Instance.new("Color3Value",Cterrain) Tcolor.Value = workspace.Terrain.WaterColor Tcolor.Name = "WaterColor"
			--Terrainmats = Enum.Material:GetEnumItems() they really cant have a seperate terrain enum huh?
			Terrainmats = {"Asphalt","Basalt","Brick","Cobblestone","Concrete","CrackedLava","Glacier","Grass","Ground","Ice","LeafyGrass","Limestone","Mud","Pavement","Rock","Salt","Sand","Sandstone","Slate","Snow","WoodPlanks",}
			for _,Tmat in pairs(Terrainmats) do
				--print(Tmat)if Tmat.Name ~= "Plastic" then --and Tmat.Name ~= "SmoothPlastic" and Tmat.Name ~= "Neon" and Tmat.Name ~= "Wood" and Tmat.Name ~= "Marble" and Tmat.Name ~= "Foil" and Tmat.Name ~= "Granite" and Tmat.Name ~= "Pebble" and Tmat.Name ~= "CorrodedMetal" and Tmat.Name ~= "DiamondPlate" and Tmat.Name ~= "Metal" and Tmat.Name ~= "Fabric" and Tmat.Name ~= "Glass" and Tmat.Name ~= "ForceField" and Tmat.Name ~= "Air" and Tmat.Name ~= "Water"  then
				--if Terrain.Material.Name == Tmat.Name then --end end 
				Cterrain:SetAttribute(Tmat,Terrain:GetMaterialColor(Tmat))
				if gethiddenproperty then colorT = Instance.new("Color3Value",Cterrain) colorT.Name = Tmat colorT.Value = Terrain:GetMaterialColor(Tmat) end
			end

		end)










