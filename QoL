					if color:IsA("ImageLabel") then 
						if Slide.Name == "Gravity" then color.ImageColor3 = colorG end 
						if Slide.Name == "Run" then color.ImageColor3 = colorR end
						if Slide.Name == "Walk" then color.ImageColor3 = colorR end
					end
				end
				if Slide.Name == "Gravity" then Sliding.BackgroundColor3 = colorG end 
				if Slide.Name == "Run" then Sliding.BackgroundColor3 = colorR end
				if Slide.Name == "Walk" then Sliding.BackgroundColor3 = colorR end

				MouseButton.MouseEnter:Connect(function() MouseisOn.Value = true end)--Point.MouseOn:TweenSize(UDim2.new(3,0,3,0),Enum.EasingDirection.Out,Enum.EasingStyle.Linear,0.12,true,nil) end)
				MouseButton.MouseLeave:Connect(function()
					MouseisOn.Value = false if MouseisDown.Value == false then Point.MouseOn:TweenSize(UDim2.new(0,0,0,0),Enum.EasingDirection.Out,Enum.EasingStyle.Linear,0.12,true,nil) end
				end)
				local function DoChangeValue()
					local NewValue = Value1.Value
					Point:TweenPosition(UDim2.new(NewValue/Max.Value,0,0.5,0),Enum.EasingDirection.Out,Enum.EasingStyle.Quad,0.1,true,nil)
					Sliding:TweenSize(UDim2.new(NewValue/Max.Value,0,1,0),Enum.EasingDirection.Out,Enum.EasingStyle.Quad,0.1,true,nil)
					Point.ValueLabel.TextLabel.Text = NewValue *1--* ValueLabelMultiply.Value
					if Slide.Name == "Gravity" then workspace.Gravity = NewValue end 
					if Slide.Name == "Jump" then char.Humanoid.JumpHeight = NewValue/2 char.Humanoid.JumpPower = NewValue end
					if Slide.Name == "Run" then char.Humanoid.WalkSpeed = NewValue end 
					if Slide.Name == "Walk" then char.Humanoid.WalkSpeed = NewValue end 
				end
				Value1:GetPropertyChangedSignal("Value"):Connect(DoChangeValue)
				Max:GetPropertyChangedSignal("Value"):Connect(DoChangeValue)
				local function SetValueFromMousePos()
					local Pos = Mouse.X - Slide.AbsolutePosition.X
					if Pos < 0 or Pos == 0 then Pos = 0 end
					if Pos > Slide.AbsoluteSize.X or Pos == Slide.AbsoluteSize.X then Pos = Slide.AbsoluteSize.X end 
					Value1.Value = math.floor((Pos/Slide.AbsoluteSize.X)*Max.Value+0.5)--if IntOnly.Value then --else Value.Value = (Pos/Slider.AbsoluteSize.X)*Max.Value end
				end
				MouseButton.MouseButton1Down:Connect(function()
					if MouseisOn.Value == true then MouseisDown.Value = true SetValueFromMousePos()
						Point:TweenSize(UDim2.new(0,14,0,14),Enum.EasingDirection.Out,Enum.EasingStyle.Quad,0.2,true,nil)
						Point.MouseDown:TweenSize(UDim2.new(3,0,3,0),Enum.EasingDirection.Out,Enum.EasingStyle.Linear,0.12,true,nil) 
						Point.ValueLabel:TweenSize(UDim2.new(0, 28,0, 40),Enum.EasingDirection.Out,Enum.EasingStyle.Linear,0.12,true,nil)--if ValueLabel.Value == true then --end
					end

				end)
				MouseButton.MouseButton1Up:Connect(function()
					MouseisDown.Value = false
				end)
				MouseButton.MouseButton2Click:Connect(function() 
					if Slide.Name == "Gravity" then workspace.Gravity = 196.2 end 
					if Slide.Name == "Jump" then char.Humanoid.JumpHeight = 7.2 char.Humanoid.JumpPower = 50 end
					if Slide.Name == "Run" then char.Humanoid.WalkSpeed = 16 end 
					if Slide.Name == "Walk" then char.Humanoid.WalkSpeed = 16 end 

				end)
				local function Button1Up()
					MouseisDown.Value = false --if Point then if Point ~= nil then --Point = Slide:WaitForChild("Point") 
					--print(Point) 
					--print(Point.Parent)
					if Point.Parent == nil then return end
					Point:TweenSize(UDim2.new(0,12,0,12),Enum.EasingDirection.Out,Enum.EasingStyle.Quad,0.2,true,nil)
					Point.MouseDown:TweenSize(UDim2.new(0,0,0,0),Enum.EasingDirection.Out,Enum.EasingStyle.Linear,0.12,true,nil)
					Point.ValueLabel:TweenSize(UDim2.new(0,0,0,0),Enum.EasingDirection.Out,Enum.EasingStyle.Linear,0.12,true,nil)--if ValueLabel.Value == true then --end
					if MouseisOn.Value == false then Point.MouseOn:TweenSize(UDim2.new(0,0,0,0),Enum.EasingDirection.Out,Enum.EasingStyle.Linear,0.12,true,nil) end
				end  

				local GuiInputService = game:GetService("UserInputService")
				GuiInputService.InputEnded:Connect(function(Key, GameProcessedEvent)
					if Key.UserInputType == Enum.UserInputType.MouseButton1 then --Button1Up() end 
						UnClick.Value = true Button1Up() 
						UnClick.Value = false
					end
				end) 


				UnClick:GetPropertyChangedSignal("Value"):Connect(function() -- this is not functional
					if UnClick.Value == true then Button1Up() end UnClick.Value = false
				end)

				--MouseButton.MouseButton1Up:Connect(Button1Up)
				--local UserInputService = game:GetService("UserInputService")
				Mouse.Move:Connect(function() if MouseisDown.Value == true then SetValueFromMousePos() end end)
			end
		end


