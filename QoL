sethiddenproperty = nil MaterialService=nil
function L_Roblox() --why do hacked clients have more flexable settings than the $5O,OOO,OOO,OOO company?

	sethiddenproperty(game.Lighting, "Technology", Enum.Technology.Voxel) sethiddenproperty(game.Lighting, "Technology", Enum.Technology.ShadowMap)
	sethiddenproperty(game.Lighting, "Technology", Enum.Technology.Future) sethiddenproperty(game.Lighting, "Technology", Enum.Technology.Compatibility) 
	sethiddenproperty(MaterialService, "Use2022Materials", true) sethiddenproperty(MaterialService, "Use2022Materials", false)
	sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", 20) game.Players.LocalPlayer.MaximumSimulationRadius = 1 game.Players.LocalPlayer.SimulationRadius = 1
	sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", 139) game.Players.LocalPlayer.MaximumSimulationRadius = 139 game.Players.LocalPlayer.SimulationRadius = 139
	sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", 5000) 
	sethiddenproperty(game.Players.LocalPlayer, "MaximumSimulationRadius", 20)
	UserSettings():GetService("UserGameSettings").ComputerCameraMovementMode = Enum.ComputerCameraMovementMode.Classic 
	settings().Rendering.QualityLevel = 3	settings().Rendering.QualityLevel = 20
	--the #### you mean you can change lighting technology, terrain chunk loading, render distance mid game on a hacked client script but NOT IN A ROBLOX SCRIPT!?! 
	--theres a couple dozen more hidden settings that aren't for my play style 
end


player = game:GetService("Players").LocalPlayer
char = player.Character or player.CharacterAdded:wait()
hum = char:WaitForChild("Humanoid") or char.Humanoid 
UserInputService = game:GetService("UserInputService")
Debris = game:GetService("Debris")
TweenService = (game:FindService("TweenService") or game:GetService("TweenService"))
RunService = game:GetService("RunService")
function RayCast(Pos, Dir, Max, IgnoreList) return game:GetService("Workspace"):FindPartOnRayWithIgnoreList(Ray.new(Pos, Dir.unit * (Max or 999.999)), IgnoreList) end
if workspace:FindFirstChild("StarFolder1") then workspace.StarFolder1:Remove() end
if player.Backpack:FindFirstChild("Epic Katana Client") then player.Backpack:FindFirstChild("Epic Katana Client"):Remove() end
if player.Backpack:FindFirstChild("Rainbow Star Sword Client") then player.Backpack:FindFirstChild("Rainbow Star Sword Client"):Remove() end

if player:FindFirstChild("disable_nds_HA") then player:FindFirstChild("disable_nds_HA"):Remove() end
if not player:FindFirstChild("disable_nds_HA") then disabled = Instance.new("BoolValue",player) disabled.Name = "disable_nds_HA" end




local controls = [[
Z = Disable Keybinds
Y = Controls
V = normal gravity
H = zero grav
J = Jupiter grav
B = moon grav
T = Platform stand,
R = Sit
C = Flip
C + Click = Flip rand
F = NDS Anti Fling
F + Click = Freeze (NDS Anti Fling)
N + Click = Fling killed
U = Swim
L + N = Remove Legs 
A + X = Change Animations
A + B = Change Run Animations

	Q + Clicks = teleport
LCtrl + Clicks = teleport

]]

function ShowAction()
	if player.PlayerGui:FindFirstChild("Action") then player.PlayerGui:FindFirstChild("Action"):Remove() end
	local Gu = Instance.new("ScreenGui",player.PlayerGui) Gu.Name = "Action" Gu.Enabled = true
	local Button = Instance.new("TextButton",Gu) Button.Name = "actions" Button.Text="" Button.Position = UDim2.new(.01, 0 , 0.3, 0) Button.Size = UDim2.new(0,  49 , 0, 36) Button.TextColor3 = Color3.fromRGB(255, 255, 255) Button.BackgroundColor3 = Color3.fromRGB(0, 0, 0) Button.BackgroundTransparency = 1  
	Button.TextXAlignment = Enum.TextXAlignment.Left Button.TextWrapped = false Button.TextScaled = false Button.TextSize = 14 Button.Active = false Button.Active = false Button.Font = Enum.Font.FredokaOne   
	actiontext = player.PlayerGui.Action.actions
	actiontext:GetPropertyChangedSignal("Text"):Connect(function() actiontext.Visible = false
		if actiontext.Parent:FindFirstChild("actionsdupe") then actiontext.Parent:FindFirstChild("actionsdupe"):Remove() end
		local dupe = actiontext:Clone() dupe.Name = "actionsdupe" dupe.Parent = actiontext.Parent dupe.Visible = true task.wait(3) dupe:Remove()
	end)
end

ProllyMobile = false ME = false
if UserInputService.TouchEnabled then ProllyMobile = true end if workspace:FindFirstChild("WeatherMachine") then ProllyMobile = true end
if ProllyMobile == true then nan = 13 else nan = 15 end
Ruse = false if   workspace:FindFirstChildWhichIsA("TerrainRegion")  then Ruse = true end  if workspace:FindFirstChildWhichIsA("BrickColorValue")  then Ruse = true end 
if Ruse == true then	disabled = player.disable_nds_HA disabled.Value = true end
_G.ClickTeleport = nil 
_G.camm = char.Humanoid.CameraOffset.Y 
--if char.Humanoid.CameraOffset.Y == 0 then camm = -1  if Ruse == true then camm = -1.75 end end
function cam()  repeat wait(1) until char:FindFirstChild("HumanoidRootPart")  _G.camm = -char.HumanoidRootPart.Size.Y  end
cam()
function ResetValues() an = 0 runan = 0 char = player.Character  ShowAction() cam()--disabled.Value = false
	if workspace:FindFirstChild("RainbowStarSwords") then if not workspace.RainbowStarSwords:IsA("Tool") then workspace:FindFirstChild("RainbowStarSwords"):Remove() end end
	char.ChildAdded:Connect(function(gear) task.wait() if gear.Name == "Rainbow Star Sword" or gear.Name == "Big Bang Sword" then disabled.Value = true game.StarterGui:SetCore("SendNotification", {Title="HA-Disabled"; Text="Press Z to Enable HA-"; Duration=5;}) end end)
	char.ChildRemoved:Connect(function(gear) task.wait() if gear.Name == "Rainbow Star Sword" or gear.Name == "Big Bang Sword" then disabled.Value = false end end)
	task.spawn(function()  task.wait(.5) 
		--task.spawn(function()  end)
		local success, data = pcall(function()
			CharSettings() 	return  
		end)
		if success then --print("no error:", data)
		else  warn("Errors:", data) end

		local success, data = pcall(function()
			Gears()  
			guias() 
			NewCamera() 
			OOF() 
			return  end)
		if success then-- print("no error:", data)
		else  warn("Errors:", data) end

	end)
end
task.wait(1) ResetValues()
player.CharacterAdded:Connect(ResetValues)


if not game.Lighting:FindFirstChild("binds") then Instance.new("BoolValue",game.Lighting).Name = "binds"
	ThisIs = {"Keybinds 1"}
	CAS = game:GetService("ContextActionService")
	function hax(actionName, inputState, inputObject)
		if inputState == Enum.UserInputState.Begin then
			if actionName == "Controls" then ShowControls()
			elseif actionName == "Toggle Keybinds" then ThisIs = {"Toggle Keybinds"}--Usefull when you dance with rainbow glow sticks that use half the keys on your keyboard...	
				disabled.Value = not disabled.Value
				if disabled.Value == true then actiontext.TextColor3 = Color3.fromRGB(255, 0, 0) actiontext.Text = "Keys Disabled, Press Z" else actiontext.TextColor3 = Color3.fromRGB(0, 255, 0) actiontext.Text = "Keys Enabled" end
			elseif actionName == "Toggle tp" then _G.ClickTeleport = not _G.ClickTeleport if _G.ClickTeleport then actiontext.TextColor3 = Color3.fromRGB(0, 255, 0) actiontext.Text = "Click teleport enabled" else actiontext.TextColor3 = Color3.fromRGB(255, 0, 0) actiontext.Text = "Press M to enable teleport" end
				--elseif actionName == "Rainbow Nova" then RainbowNova()
			end
			if disabled.Value == false then if actiontext.TextColor3 ~= Color3.fromRGB(255, 255, 255) then actiontext.TextColor3 = Color3.fromRGB(255, 255, 255) end	
				if actionName == "HAX" then workspace.Gravity = 126.2 char.Humanoid.JumpPower = 50 char.Humanoid.JumpHeight = 11 actiontext.Text = "hax"
				elseif actionName == "Anti Fling HAX" then AntiFling() 
				elseif actionName == "RAINBOW STAR SWORDS" then RainbowStarSword()
				elseif actionName == "Dog Walk" then Swim() --dog walk only works with werewolf swim animation
				elseif actionName == "Default Gravity" then workspace.Gravity = 196.2 char.Humanoid.JumpPower = 50 actiontext.Text = "Default Gravity" if Spaghetti and Spaghetti:FindFirstChild("fps 60") then Spaghetti:FindFirstChild("fps 60").fps.Value = 900 end
				elseif actionName == "Zero Gravity" then workspace.Gravity = 0 actiontext.Text = "Zero Gravity"
				elseif actionName == "Moon Gravity" then workspace.Gravity = 32.41 actiontext.Text = "Moon Gravity" char.Humanoid.JumpPower = 25 char.Humanoid.JumpHeight = 11
				elseif actionName == "Jupiter Gravity" then workspace.Gravity = 111111 actiontext.Text = "Jupiter Gravity"
				elseif actionName == "Low2" then workspace.Gravity = 5 char.Humanoid.JumpPower = 11 actiontext.Text = "Low Gravity2" 
				elseif actionName == "Low" then workspace.Gravity = 15 char.Humanoid.JumpPower = 17 actiontext.Text = "Low Gravity" --if ME == true then char.Humanoid.WalkSpeed = 11 end
				elseif actionName == "Sit" then player.Character.Humanoid.Sit = not player.Character.Humanoid.Sit actiontext.Text = "Sit" 
				elseif actionName == "PlatformStand" then player.Character.Humanoid.PlatformStand = not player.Character.Humanoid.PlatformStand actiontext.Text = "PlatformStand"
				elseif actionName == "Dance List" then player.PlayerGui.MAMALUIGI.Button["TIME TO DIY 3"].Enabled = not player.PlayerGui.MAMALUIGI.Button["TIME TO DIY 3"].Enabled actiontext.Text = "Dance"
				elseif actionName == "AirBlock" then AirBlock() 
				elseif actionName == "A spot" then  local success, data = pcall(function()
						tpval = game.Lighting.save4.Value tpvector()  	return  end)
					if data then  warn("Errors:", data) end 
				elseif actionName == "SEAT no nds tp" then SEAT() 
				elseif actionName == "Mirror" then  Mirror() --char:PivotTo(char.HumanoidRootPart.CFrame * CFrame.Angles(0,math.rad(180),0))
				end
			end 
		end
	end 
	--CAS:BindAction("v1", hax, false, Enum.KeyCode.Y)
	--CAS:BindAction("SEAT no nds tp", hax, false, Enum.KeyCode.Y)
	--CAS:BindAction("Controls", hax, false, Enum.KeyCode.Y)
	CAS:BindAction("Toggle Keybinds", hax, false, Enum.KeyCode.Z)
	--CAS:BindAction("Toggle tp", hax, false, Enum.KeyCode.Y)
	CAS:BindAction("Low", hax, false, Enum.KeyCode.X)
	CAS:BindAction("HAX", hax, false, Enum.KeyCode.G)
	--CAS:BindAction("Mirror", hax, false, Enum.KeyCode.E)
	CAS:BindAction("Anti Fling HAX", hax, false, Enum.KeyCode.F)
	--CAS:BindAction("Rainbow Nova", hax, false, Enum.KeyCode.F)
	--CAS:BindAction("RAINBOW STAR SWORDS", hax, false, Enum.KeyCode.U)
	CAS:BindAction("Dog Walk", hax, false, Enum.KeyCode.K)
	CAS:BindAction("Zero Gravity", hax, false, Enum.KeyCode.H)
	CAS:BindAction("Low Gravity2", hax, false, Enum.KeyCode.B)
	CAS:BindAction("Default Gravity", hax, false, Enum.KeyCode.V)
	CAS:BindAction("Jupiter Gravity", hax, false, Enum.KeyCode.J) 
	L = Enum.KeyCode.R if Ruse == true then L = Enum.KeyCode.B end CAS:BindAction("Sit", hax, false, L)
	CAS:BindAction("PlatformStand", hax, false, Enum.KeyCode.T) 
	CAS:BindAction("Dance List", hax, false, Enum.KeyCode.Period,Enum.KeyCode.Comma)
	CAS:BindAction("AirBlock", hax, false, Enum.KeyCode.M)
	CAS:BindAction("A spot", hax, false, Enum.KeyCode.U)
	if disabled.Value == true then actiontext.Text = [[HA-Disabled"
Press Z to Enable HA-]] else actiontext.Text = [[HA-Enabled"
Press Z to Disable HA-]] end

	ThisIsServerSided = {"Setup tp"}
	if _G.ClickTeleport == nil then _G.ClickTeleport = true
		if _G.ClickTeleport then --game.StarterGui:SetCore("SendNotification", {Title="WeAreDevs.net"; Text="Click teleport enabled"; Duration=5;})
		else game.StarterGui:SetCore("SendNotification", {Title="to tp"; Text="Press U to enable teleport"; Duration=5;}) end
		mouse = player:GetMouse() repeat wait() until mouse
		UserInputService.InputBegan:Connect(function(input, gameProcessed)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then 
				--if _G.ClickTeleport and UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) then tpOnPoint() end
				if _G.ClickTeleport and UserInputService:IsKeyDown(Enum.KeyCode.Q) then tpOnPoint() end 
				if _G.ClickTeleport and UserInputService:IsKeyDown(Enum.KeyCode.E) then tpCarOnPoint() end 
			end
			if input.UserInputType == Enum.UserInputType.MouseButton2 then 
				--if _G.ClickTeleport and UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) then tpAbovePoint() end
				if _G.ClickTeleport and UserInputService:IsKeyDown(Enum.KeyCode.Q) then tpAbovePoint() end 
				if _G.ClickTeleport and UserInputService:IsKeyDown(Enum.KeyCode.E) then tpCarAbovePoint() end 
			end
		end)
		--else if Ruse == true then _G.ClickTeleport = false end
	end

	ThisIs = {"Keybinds 2"}
	UserInputService.InputBegan:Connect(function(input, gameProcessethend)

		ThisIsServerSided = {"spin + explode"}
		if input.UserInputType == Enum.UserInputType.MouseButton1 then if UserInputService:IsKeyDown(Enum.KeyCode.N) then _G.flipspin() end end 
		if input.UserInputType == Enum.UserInputType.MouseButton2 then if UserInputService:IsKeyDown(Enum.KeyCode.N) then _G.nilspin2() end end 
		if UserInputService:IsKeyDown(Enum.KeyCode.N) then _G.nilspin() end 

		if disabled.Value == false then
			ThisIsServerSided = {"flip"}
			if UserInputService:IsKeyDown(Enum.KeyCode.C) then if char.Humanoid.SeatPart then for _,seat in pairs(char.Humanoid.SeatPart:GetConnectedParts(true)) do if seat.Anchored then print("seat is anchored") return end end end
				char:PivotTo(char.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(180),0,0))
				game.StarterGui:SetCore("SendNotification", {Title="flip"; Text= "click + C = rand CFrame" ; Duration=1;}) end
			ThisIsServerSided = {"flipRandom"}
			if input.UserInputType == Enum.UserInputType.MouseButton1 then 
				if UserInputService:IsKeyDown(Enum.KeyCode.C) then if char.Humanoid.SeatPart then for _,seat in pairs(char.Humanoid.SeatPart:GetConnectedParts(true)) do if seat.Anchored then print("seat is anchored") return end end end
					char:PivotTo(char.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(math.random( 0,360)),math.rad(math.random( 0,360)),math.rad(math.random( 0,360))))
				end
			end
			if input.UserInputType == Enum.UserInputType.MouseButton2 then 
				if UserInputService:IsKeyDown(Enum.KeyCode.C) then if char.Humanoid.SeatPart then for _,seat in pairs(char.Humanoid.SeatPart:GetConnectedParts(true)) do if seat.Anchored then print("seat is anchored") return end end end
					--char:PivotTo(char.HumanoidRootPart.CFrame * CFrame.Angles(0,0,math.rad(180))) 
					char:PivotTo(  workspace.CurrentCamera.CFrame ) 
				end
			end

			ThisIsServerSided = {"antifling"}
			if input.UserInputType == Enum.UserInputType.MouseButton1 then if UserInputService:IsKeyDown(Enum.KeyCode.F) then AntiFlingPlayable() end end 



			ThisIsServerSided = {"Remove Legs to be floaty", } --no take backs
			if UserInputService:IsKeyDown(Enum.KeyCode.N) and UserInputService:IsKeyDown(Enum.KeyCode.L) then HIDE_LEGS() end--REMOVE_LEGS() end
			if UserInputService:IsKeyDown(Enum.KeyCode.L) and UserInputService:IsKeyDown(Enum.KeyCode.N) then HIDE_LEGS() end--REMOVE_LEGS() end

			local ThisIsServerSided = {"Animation Groups" }
			if UserInputService:IsKeyDown(Enum.KeyCode.A) and UserInputService:IsKeyDown(Enum.KeyCode.X) then AnimSet() end 
			if UserInputService:IsKeyDown(Enum.KeyCode.A) and UserInputService:IsKeyDown(Enum.KeyCode.B) then AnimRun() end 

			if input.UserInputType == Enum.UserInputType.MouseButton1 then if UserInputService:IsKeyDown(Enum.KeyCode.E) then Mirror() end end 


			--if input.UserInputType == Enum.UserInputType.MouseButton1 then if UserInputService:IsKeyDown(Enum.KeyCode.V) then char.Humanoid.WalkSpeed = 16 end end 
			--if input.UserInputType == Enum.UserInputType.MouseButton2 then if UserInputService:IsKeyDown(Enum.KeyCode.V) then char.Humanoid.WalkSpeed = 24 end end 
			--if input.UserInputType == Enum.UserInputType.MouseButton1 then if UserInputService:IsKeyDown(Enum.KeyCode.G) then char.Humanoid.WalkSpeed = 20 end end 
			--if input.UserInputType == Enum.UserInputType.MouseButton2 then if UserInputService:IsKeyDown(Enum.KeyCode.G) then char.Humanoid.WalkSpeed = 5 end end 
		end
	end) 
end


function AntiFling() 
	player.Character.HumanoidRootPart.Anchored = not player.Character.HumanoidRootPart.Anchored 
	ThisIs = [[ Unbelievable! Fling hackers ruined my day plenty of times, but you know what they say "after a storm comes a rainbow"
				because I wouldn't have made this single line of code without them hackers, and this line of code is the reason this entire script exists... ]]
	--funny part is roblox ran all of MY executors out of business, so now i have no defence against fortnite hackers who pull executors out of their as, and fling me. Good Job Roblox...

	for _,v in pairs(char:GetChildren()) do if v:IsA("BasePart") then v.Velocity = Vector3.new(0,0,0) v.RotVelocity = Vector3.new(0,0,0) end end
	if player.Character.HumanoidRootPart.Anchored == true then game.StarterGui:SetCore("SendNotification", {Title="anchored"; Text= "" ; Duration=2;}) else
		game.StarterGui:SetCore("SendNotification", {Title="unanchored"; Text= "" ; Duration=2;}) end 
end

nofling = false
function AntiFlingPlayable() 
	The = {"First video Sakpot posts as soon as he gets my favorite executor working is 2O minutes of fling killing people on NDS, what a smack in the face :) "}
	ThisIs = {"more anti fling methods"}--these have not been tested against fling hackers yet
	for _,v in pairs(workspace:GetDescendants()) do 
		if v:IsA("BasePart") and not v:IsDescendantOf(char) then if v.Name == "HumanoidRootPart" or v.Name == "Torso" or v.Name == "UpperTorso" or v.Name == "LowerTorso" or v.Name == "Head" then v.CanCollide = false end end --I'm assuming that the hacker makes their RootPart spin really fast to smack you, depending on how good your client ping is you might be able to walk through the hacker without being flinged
		if v:IsA("BasePart") and v.Parent:IsA("Model") and v.Parent:FindFirstChild("HumanoidRootPart") and not v:IsDescendantOf(char) then v.CanCollide = false end --I'm assuming that this makes you walk through the hacker without being flinged
	end 
	for _,v in pairs(char:GetChildren()) do if v:IsA("BasePart") then v.Velocity = Vector3.new(0,0,0) v.RotVelocity = Vector3.new(0,0,0) v.CanTouch = not v.CanTouch end end -- you will stop flying to space, might still take fall damage -- create velocity magnetude tween to negate fall damage
	--ThisIs = {"Blast Protection V"} for _,v in pairs(char:GetChildren()) do if v:IsA("BasePart") then v.CustomPhysicalProperties = PhysicalProperties.new(100, 1, 0, .3, 1) end end --this multiplies your character mass by 14O, I am tiny so this isn't a consistant form of antifling  
	--RunService.Stepped:connect(function() if nofling then char.Humanoid:ChangeState(11) end end)
	ThisIs = {"noclip... yes, it took me 6 years to realize that I can use noclip to walk through the fling hackers"}
	nofling = not nofling RunService.Stepped:connect(function() 
		if nofling == true then 
			if char.Humanoid.Sit == true or char.Humanoid.PlatformStand == true or char.Humanoid.Health<1 then 
				nofling = false char.Humanoid.Sit = false char.Humanoid.PlatformStand = false game.StarterGui:SetCore("SendNotification", {Title="nofling off"; Text= "" ; Duration=2;}) end
			for _,v in pairs(char:GetChildren()) do if v:IsA("BasePart") then 
					if v.Name == "HumanoidRootPart" or v.Name == "Torso" or v.Name == "UpperTorso" or v.Name == "LowerTorso" then v.CanCollide = false end end end end 
	end) if nofling == true then game.StarterGui:SetCore("SendNotification", {Title="nofling"; Text= "" ; Duration=2;}) else 
		game.StarterGui:SetCore("SendNotification", {Title="nofling off"; Text= "" ; Duration=2;})  for _,v in pairs(char:GetChildren()) do if v:IsA("BasePart") then v.CanTouch = not v.CanTouch
				if v.Name == "HumanoidRootPart" or v.Name == "Torso" or v.Name == "UpperTorso" or v.Name == "LowerTorso" then v.CanCollide = true end end end wait(.5) char.HumanoidRootPart.CanCollide = true 
	end
end

mover = nil bayblade = nil bayblade2 = nil smack = nil rotat = nil wind = nil 
if not _G.nds then _G.nds = true
	function _G.FLing_Killed_Far()   char.HumanoidRootPart.Anchored = false --char.UpperTorso.RotVelocity = Vector3.new( math.random(5,10)*100, math.random(5,10)*100, math.random(5,15)*100)
		mover = Vector3.new( math.random(-100,100)*50, math.random(80,100)*20, math.random(50,100)*200)
		bayblade = Vector3.new( math.random(5,10)*100, math.random(5,10)*100, math.random(5,15)*100)
		char.HumanoidRootPart.RotVelocity = bayblade
		if char.Humanoid.SeatPart  then char.Humanoid.SeatPart.RotVelocity = bayblade*50 task.wait(math.random(110,260)/100) end
		if char.Animate.Enabled == false  then char.HumanoidRootPart.RotVelocity = bayblade*50 char.UpperTorso.RotVelocity = bayblade*50 end  

		char.HumanoidRootPart.Velocity = mover char.UpperTorso.Velocity = mover
		char.Humanoid.Sit = true task.wait(math.random(40,60)/100) char.Humanoid.Health = - 100999 
		smack = Instance.new("Sound",char.HumanoidRootPart) smack.SoundId = "rbxassetid://6029917449" smack.RollOffMinDistance = 1000000 smack.RollOffMaxDistance = 1000000 smack:Play()
		game.StarterGui:SetCore("SendNotification", {Title="fling killed"; Text= "u r in space" ; Duration=1;})
	end
	function _G.FLing_Killed() char.HumanoidRootPart.Anchored = false -- because OOOooOOoUFGZRFF = peak entertainment
		--char.UpperTorso.RotVelocity = Vector3.new( math.random(5,10)*100, math.random(5,10)*100, math.random(5,15)*100) 
		bayblade2 = Vector3.new( math.random(50,100)*3, math.random(50,100)*3, math.random(50,100)*3)
		char.HumanoidRootPart.RotVelocity = bayblade2 
		char.UpperTorso.RotVelocity = bayblade2
		if char.Humanoid.SeatPart then char.Humanoid.SeatPart.RotVelocity = bayblade2*50 char.UpperTorso.RotVelocity = bayblade2*50 char.HumanoidRootPart.RotVelocity = bayblade2*50 task.wait(math.random(110,260)/100) end
		--char.HumanoidRootPart.Velocity = Vector3.new( math.random(-25,25)/10, math.random(25,25)/10, math.random(25,25)/10) char.UpperTorso.Velocity = char.HumanoidRootPart.Velocity
		char.Humanoid.Sit = true task.wait(math.random(40,60)/100) char.Humanoid.Health = - 100999 
		smack = Instance.new("Sound",char.HumanoidRootPart) smack.SoundId = "rbxassetid://6029917449" smack.RollOffMinDistance = 1000000 smack.RollOffMaxDistance = 1000000 smack:Play()
		game.StarterGui:SetCore("SendNotification", {Title="fling killed"; Text= "u r in space" ; Duration=1;})
	end
	function _G.nilspinmove()  
		rotat = Vector3.new( math.random(-100,100)/66, math.random(-100,100)/66, math.random(-100,100)/66 ) 
		char.HumanoidRootPart.RotVelocity = rotat char.UpperTorso.RotVelocity = rotat 
		--if char.Humanoid.SeatPart then char.Humanoid.SeatPart.RotVelocity = rotat*50 end else
		--wind = Instance.new("BodyAngularVelocity", char.UpperTorso) wind.AngularVelocity = Vector3.new(math.random(5,50), math.random(5,50), math.random(5,50))
		--wind.MaxTorque = Vector3.new(math.random(-10,10), math.random(-10,10), math.random(-10,10)) wait(.01) wind:Remove() end
	end
	function _G.nilspin()  
		rotat = Vector3.new( math.random(-100,100)/66, math.random(-100,100)/66, math.random(-100,100)/66 ) 
		char.HumanoidRootPart.RotVelocity = rotat --char.UpperTorso.RotVelocity = rotat 
		--if char.Humanoid.SeatPart then char.Humanoid.SeatPart.RotVelocity = rotat*50 end else
		--wind = Instance.new("BodyAngularVelocity", char.UpperTorso) wind.AngularVelocity = Vector3.new(math.random(5,50), math.random(5,50), math.random(5,50))
		--wind.MaxTorque = Vector3.new(math.random(-10,10), math.random(-10,10), math.random(-10,10)) wait(.01) wind:Remove() end
	end
	function _G.nilspin2()  
		rotat = Vector3.new( math.random(-100,100)/6, math.random(-100,100)/6, math.random(-100,100)/6 ) 
		char.HumanoidRootPart.RotVelocity = rotat end
	function _G.flipspin()  
		rotat = Vector3.new( math.random(-100,100)/66, 0, 0 ) 
		char.HumanoidRootPart.RotVelocity = rotat
		--rotat = Vector3.new( math.random(-100,100)/66, 0, 0 ) 
		--char.HumanoidRootPart.RotVelocity = rotat --char.UpperTorso.RotVelocity = rotat 
		--if char.Humanoid.SeatPart then char.Humanoid.SeatPart.RotVelocity = rotat*50 end else

		--local wind = Instance.new("AngularVelocity", char.HumanoidRootPart.RootAttachment) wind.AngularVelocity = Vector3.new(math.random(-10,10), 0, 0) wind.Attachment0 = char.HumanoidRootPart.RootAttachment
		--wind.RelativeTo = Enum.ActuatorRelativeTo.Attachment0  wind.ReactionTorqueEnabled = true	wind.MaxTorque = math.random(50,100) wait(1) wind:Remove() 
	end

end
tptohide1 = nil tptohide2 = nil tptodone = nil tpto = nil modleh = nil 
function tpvector() char.HumanoidRootPart.Anchored = false if sethiddenproperty then player.CameraMaxZoomDistance = 1000 end --if seatcheck == true then return end 
	if char.Humanoid.SeatPart then for _,seat in pairs(char.Humanoid.SeatPart:GetConnectedParts(true)) do if seat.Anchored then print("seat is anchored") return end end  end
	for _,v in pairs(char:GetChildren()) do if v:IsA("BasePart") then v.Velocity = Vector3.new(0,0,0) v.RotVelocity = Vector3.new(0,0,0) end end
	if not char.Humanoid.SeatPart then
		tptohide1 = Instance.new("Part") tptohide1.CanCollide = false tptohide1.CFrame = char.HumanoidRootPart.CFrame tptohide1.Position = Vector3.new(tptohide1.Position.X, 5, tptohide1.Position.Z) 
		char:PivotTo(tptohide1.CFrame) tptohide1:Remove() task.wait(.1) os.time()
		tptohide2 = Instance.new("Part") tptohide2.CanCollide = false tptohide2.CFrame = char.HumanoidRootPart.CFrame tptohide2.Position = Vector3.new(tpval.X, 5, tpval.Z) 
		char:PivotTo(tptohide2.CFrame) tptohide2:Remove() task.wait(.1) os.time()
	end
	--this is the important one
	task.wait(.1) os.time()
	tptodone = Instance.new("Part") tptodone.CanCollide = false tptodone.CFrame = char.HumanoidRootPart.CFrame tptodone.Position = tpval --+ Vector3.new(0,1,0)
	char:PivotTo(tptodone.CFrame) tptodone:Remove()
end

function tpOnPoint() if sethiddenproperty then player.CameraMaxZoomDistance = 1000 end
	if char.Humanoid.SeatPart then for _,seat in pairs(char.Humanoid.SeatPart:GetConnectedParts(true)) do if seat.Anchored then print("seat is anchored") return end end end
	for _,v in pairs(char:GetChildren()) do if v:IsA("BasePart") then v.Velocity = Vector3.new(0,0,0) v.RotVelocity = Vector3.new(0,0,0) end end
	tpto = Instance.new("Part") tpto.CanCollide = false tpto.CFrame = char.HumanoidRootPart.CFrame tpto.Position = Vector3.new(mouse.Hit.x, mouse.Hit.y+1, mouse.Hit.z)
	char:PivotTo(tpto.CFrame) tpto:Remove()
end
function tpAbovePoint() if sethiddenproperty then player.CameraMaxZoomDistance = 1000 end
	if char.Humanoid.SeatPart then for _,seat in pairs(char.Humanoid.SeatPart:GetConnectedParts(true)) do if seat.Anchored then print("seat is anchored") return end end end
	for _,v in pairs(char:GetChildren()) do if v:IsA("BasePart") then v.Velocity = Vector3.new(0,0,0) v.RotVelocity = Vector3.new(0,0,0) end end
	modleh = Instance.new("Model",workspace)
	tpto = Instance.new("Part",modleh) tpto.Anchored = true tpto.Size = Vector3.new(5,5,5)  tpto.CanCollide = true tpto.Color = Color3.fromRGB(0, 255, 0) tpto.Material = Enum.Material.Neon
	tpto.CFrame = char.HumanoidRootPart.CFrame tpto.Position = Vector3.new(mouse.Hit.x, mouse.Hit.y+1, mouse.Hit.z)
	modleh:MoveTo(Vector3.new(mouse.Hit.x, mouse.Hit.y, mouse.Hit.z))
	char:PivotTo(tpto.CFrame)   modleh:Remove()	
end
function tpCarOnPoint() if not char.Humanoid.SeatPart then return end
	for _,seat in pairs(char.Humanoid.SeatPart:GetConnectedParts(true)) do if seat.Anchored then print("seat is anchored") return end end
	if char.Humanoid.SeatPart.Parent.Parent:IsA("Model") then car = char.Humanoid.SeatPart.Parent.Parent end 
	if char.Humanoid.SeatPart.Parent:IsA("Model") then car = char.Humanoid.SeatPart.Parent end 
	--	tpto = Instance.new("Part") tpto.CanCollide = false tpto.CFrame = car:GetPivot() tpto.Position = Vector3.new(mouse.Hit.x, mouse.Hit.y+5, mouse.Hit.z)
	car:PivotTo( car:GetPivot()*CFrame.new(mouse.Hit.x, mouse.Hit.y+5, mouse.Hit.z)) --tpto:Remove()
end
function tpCarAbovePoint() if not char.Humanoid.SeatPart then return end
	for _,seat in pairs(char.Humanoid.SeatPart:GetConnectedParts(true)) do if seat.Anchored then print("seat is anchored") return end end
	if char.Humanoid.SeatPart.Parent.Parent:IsA("Model") then car = char.Humanoid.SeatPart.Parent.Parent end 
	if char.Humanoid.SeatPart.Parent:IsA("Model") then car = char.Humanoid.SeatPart.Parent end
	modleh = Instance.new("Model",workspace) tpto = Instance.new("Part",modleh) tpto.Anchored = true tpto.Size = Vector3.new(5,5,5)  tpto.CanCollide = true tpto.Color = Color3.fromRGB(0, 255, 0) tpto.Material = Enum.Material.Neon
	tpto.CFrame = char.Humanoid.SeatPart tpto.Position = Vector3.new(mouse.Hit.x, mouse.Hit.y+2, mouse.Hit.z)
	modleh:MoveTo(Vector3.new(mouse.Hit.x, mouse.Hit.y, mouse.Hit.z))
	car:PivotTo(modleh:GetPivot())  -- modleh:Remove()	
end

function Mirror() if Ruse == false then
		if workspace.CurrentCamera:FindFirstChild("L") then workspace.CurrentCamera:FindFirstChild("L"):Remove() return end
		if not workspace.CurrentCamera:FindFirstChild("L") then
			local l__CurrentCamera__3 = workspace.CurrentCamera;
			local v12 = Instance.new("Part"); v12.Transparency = 1;  v12.Anchored = true; v12.Shape = Enum.PartType.Ball; v12.CanCollide = false; 
			v12.Parent = l__CurrentCamera__3; v12.CFrame = l__CurrentCamera__3.CFrame;
			v12.Position = char.HumanoidRootPart.Position 
			char:PivotTo(v12.CFrame* CFrame.new(0,0, 0)*CFrame.Angles(0,math.rad(-180),0)) v12:Remove()
		end end
end

