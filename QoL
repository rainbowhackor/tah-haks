				if mouse.Hit.Y > -100 and mouse.Hit.Y < 100 then yq = math.floor(mouse.Hit.Y/10 * 100 )/10 else yq = math.floor(mouse.Hit.Y) end
				if mouse.Hit.Z > -1000 and mouse.Hit.Z < 1000 then zq = math.floor(mouse.Hit.Z/10 * 100 )/10 else zq = math.floor(mouse.Hit.Z) end
