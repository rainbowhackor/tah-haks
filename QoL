nocol = nil Trans2 = nil
function RainbowStarSword() --( glow sticks for dancing ) OH MY GAH THIS LOOKS SO BAD in mobile emulator / Compatibility Lighting! but thats 9O% of games i play :[
	if workspace:FindFirstChild("RainbowStarSwords") then workspace:FindFirstChild("RainbowStarSwords"):Remove() return end

	if not workspace:FindFirstChild("StarFolder1") then starfolder = Instance.new("Folder",workspace) starfolder.Name = "StarFolder1"
		CappedGraphics = Instance.new("BoolValue",starfolder) CappedGraphics.Name = "CappedGraphics" if ProllyMobile == true then CappedGraphics.Value = true end
		IvoryShard1 = Instance.new("BoolValue",starfolder) IvoryShard1.Name = "IvoryShard1" IvoryShard1.Value = true 
		IvoryShard2 = Instance.new("BoolValue",starfolder) IvoryShard2.Name = "IvoryShard2"
		RainbowShard1 = Instance.new("BoolValue",starfolder) RainbowShard1.Name = "RainbowShard1"
		Beams = Instance.new("BoolValue",starfolder) Beams.Name = "Beams" Beams.Value = false 
		RainbowBeams = Instance.new("BoolValue",starfolder) RainbowBeams.Name = "RainbowBeams"
		BeamSpam = Instance.new("BoolValue",starfolder) BeamSpam.Name = "BeamSpam"
		ShardSpam = Instance.new("BoolValue",starfolder) ShardSpam.Name = "ShardSpam"
		NoBeamLight = Instance.new("BoolValue",starfolder) NoBeamLight.Name = "NoBeamLight" NoBeamLight.Value = true --Beam Lights are extremely laggy
		SmoothColor = Instance.new("BoolValue",starfolder) SmoothColor.Name = "SmoothColor"
		--OriginalFlares = Instance.new("BoolValue",starfolder) OriginalFlares.Name = "OriginalFlare"
		RainbowBeamSpam = Instance.new("BoolValue",starfolder) RainbowBeamSpam.Name = "RainbowBeamSpam"
	end

	RainbowStarSwords = Instance.new("Folder",workspace) RainbowStarSwords.Name = "RainbowStarSwords"
	sword = Instance.new("Part",RainbowStarSwords) sword.Name = "Sword1" sword.Size = Vector3.new(1, 0.6, 5.2) sword.CanTouch = false sword.CanCollide = false sword.Massless = true sword.CanQuery = false sword.CastShadow = false
	bagodx = Instance.new("SpecialMesh",sword) bagodx.MeshId = "http://www.roblox.com/asset/?id=80557857" bagodx.TextureId = "rbxassetid://157345185" bagodx.VertexColor = Vector3.new(2, 2, 2)
	bottomatt = Instance.new("Attachment",sword) bottomatt.Name = "bottomatt" bottomatt.Position = Vector3.new(0, 0, -2) bottomatt.Orientation = Vector3.new(0, 90, 90) 
	constraint = Instance.new("RigidConstraint",sword) constraint.Attachment0 = bottomatt constraint.Attachment1 = char.RightHand.RightGripAttachment

	TopTrail = Instance.new("Attachment",sword) TopTrail.Name = "TopTrail" TopTrail.Position = Vector3.new(0, 0, 2.6)   
	BottomTrail = Instance.new("Attachment",sword) BottomTrail.Name = "BottomTrail" BottomTrail.Position = Vector3.new(0, 0, -0.7) 
	Trail = Instance.new("Trail",sword) Trail.Name = "Trail" Trail.Brightness=1 Trail.TextureMode = Enum.TextureMode.Wrap Trail.Color = ColorSequence.new{
		ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 0, 0)),
		ColorSequenceKeypoint.new(0.169, Color3.fromRGB(255, 85, 0)),
		ColorSequenceKeypoint.new(0.33, Color3.fromRGB(255, 255, 0)),
		ColorSequenceKeypoint.new(0.5, Color3.fromRGB(0, 255, 0)),
		ColorSequenceKeypoint.new(0.66, Color3.fromRGB(0, 85, 255)),
		ColorSequenceKeypoint.new(0.83, Color3.fromRGB(0, 0, 255)),
		ColorSequenceKeypoint.new(1, Color3.fromRGB(170, 0, 255))}
	Trail.LightEmission=1 Trail.LightInfluence=0 
	Trail.Transparency = NumberSequence.new{ NumberSequenceKeypoint.new(0,1),NumberSequenceKeypoint.new(.1,.75),NumberSequenceKeypoint.new(1,1)}
	Trail.Attachment0 = TopTrail Trail.Attachment1 = BottomTrail Trail.Lifetime=1 Trail.WidthScale = NumberSequence.new(5) Trail.Enabled = true

	Flare:Clone().Parent = BottomTrail
	OriginalFlare:Clone().Parent = BottomTrail 
	BigStar:Clone().Parent = sword 
	--Stardust = Stars:Clone() Stardust.Parent = sword Stardust.Lifetime = NumberRange.new(.5, 2)  
	sword2 = sword:Clone() sword2.Name = "Sword2" sword2.Parent = RainbowStarSwords sword2.RigidConstraint.Attachment1 = char.LeftHand.LeftGripAttachment
	Stars:Clone().Parent = sword

	rlight = Instance.new("PointLight",sword) rlight.Range = 20 rlight.Brightness = .5 --if game.Lighting.Technology == Enum.Technology.Future then rlight.Brightness = 1 end
	rSparkles = Instance.new("Sparkles",sword)

	The = {" Rainbow Color Changing "} if Ruse == false then
		colorcwrap2 = coroutine.create(function() while workspace:FindFirstChild("RainbowStarSwords") do RunService.RenderStepped:Wait() if starfolder.SmoothColor.Value == false then
					task.wait(math.random(35,70)/100) end rlight.Color = ColorChanging.Value rSparkles.SparkleColor = ColorChanging.Value end end) coroutine.resume(colorcwrap2)
	end
	--[[The = {" Rainbow Light Colors "}
	lightcwrap = coroutine.create(function()  
		while workspace:FindFirstChild("RainbowStarSwords") do 
			for _,Colors in pairs(ColorCycle) do 
				rTween = TweenService:Create(rlight,LightFade,{Color = Colors}) 
				rTween:Play() rTween.Completed:Wait()
				if not workspace:FindFirstChild("RainbowStarSwords") then if rTween then rTween:Pause() rTween:Remove() end return end  end
		end
	end) coroutine.resume(lightcwrap)
	The = {" Rainbow Sparkle Colors "}
	task.spawn(function()
		while workspace:FindFirstChild("RainbowStarSwords") do
				for _,Colors in pairs(ColorCycle) do 
				rTween2 = TweenService:Create(rSparkles,LightFade,{SparkleColor = Colors}) rTween2:Play() rTween2.Completed:Wait() 
				if not workspace:FindFirstChild("RainbowStarSwords") then if rTween2 then rTween2:Pause() rTween2:Remove() end return  end
				end --
			end
	end)]]

	The = {" Star Shards "}
	shardcwrap1 = coroutine.create(function() repeat RunService.RenderStepped:Wait()
			if RainbowStarSwords and starfolder.IvoryShard1.Value == true then 
				Proj = Shard:Clone() Debris:AddItem(Proj,1.33) Proj.CFrame = sword.CFrame Proj.Parent = starfolder Shard.Sparkle:Play()--Proj:SetNetworkOwner(nil)
				Proj.Velocity = CFrame.new(sword.CFrame.p,sword.CFrame.p+Vector3.new(Seed:NextNumber(-1,1),Seed:NextNumber(-1,1),Seed:NextNumber(-1,1))).lookVector*Seed:NextNumber(50,70)
			end 
			if starfolder.ShardSpam.Value == true then task.wait(math.random(05,11)/100) else task.wait(math.random(33,55)/100) end 
		until not workspace:FindFirstChild("RainbowStarSwords") 
	end) coroutine.resume(shardcwrap1)
	shardcwrap2 = coroutine.create(function() repeat RunService.RenderStepped:Wait()
			if RainbowStarSwords and starfolder.IvoryShard2.Value == true then 
				Proj = Shard:Clone() Debris:AddItem(Proj,1.33) Proj.CFrame = sword2.CFrame Proj.Parent = starfolder Shard.Sparkle:Play()--Proj:SetNetworkOwner(nil)
				Proj.Velocity = CFrame.new(sword2.CFrame.p,sword2.CFrame.p+Vector3.new(Seed:NextNumber(-1,1),Seed:NextNumber(-1,1),Seed:NextNumber(-1,1))).lookVector*Seed:NextNumber(50,70)
			end 
			if starfolder.ShardSpam.Value == true then task.wait(math.random(05,11)/100) else task.wait(math.random(33,55)/100) end 
		until not workspace:FindFirstChild("RainbowStarSwords") 
	end) coroutine.resume(shardcwrap2)

	The = {" Crimson Beams "}
	Root = char:FindFirstChild("HumanoidRootPart")
	beamcwrap1 = coroutine.create(function() repeat RunService.RenderStepped:Wait()
			if RainbowStarSwords and starfolder.Beams.Value == true then 
				local SpawnPos = (Root.CFrame*CFrame.new(Seed:NextInteger(-40,40),100,Seed:NextInteger(-40,40))).p
				local hit,pos = RayCast(SpawnPos,(SpawnPos-Vector3.new(0,1,0))-SpawnPos,150,{char})
				if hit then

					if starfolder.NoBeamLight.Value == false then PillarClone1 = RedPillar:Clone() else PillarClone1 = RedPillarNoLight:Clone() end
					local PillarClone = PillarClone1 PillarClone.CFrame = CFrame.new(pos+Vector3.new(0,PillarClone.Size.X/2,0))*CFrame.Angles(0,0,math.rad(90)) 
					PillarClone.Parent = starfolder

					task.spawn(function() task.wait(.5)
						LaserSound = PillarSound:Clone() LaserSound.Parent = PillarClone
						PillarTween = TweenService:Create(PillarClone,TweenInfo.new(.5,Enum.EasingStyle.Linear,Enum.EasingDirection.Out,0,false,0),{Transparency = 1,Size = Vector3.new(PillarClone.Size.X,20,20),Color = Color3.fromRGB(255, 0, 255)})
						SoundTween = TweenService:Create(LaserSound,TweenInfo.new(.5,Enum.EasingStyle.Linear,Enum.EasingDirection.Out,0,false,0),{Volume = 0})

						for _,light in pairs(PillarClone:GetChildren()) do
							if light:IsA("Light") then local LightTween = TweenService:Create(light,TweenInfo.new(.5,Enum.EasingStyle.Linear,Enum.EasingDirection.Out,0,false,0),{Color = Color3.fromRGB(255, 0, 255)}) LightTween:Play() end end 
						LaserSound:Play() PillarTween:Play() SoundTween:Play() Debris:AddItem(PillarClone,.5) --SoundTween.Completed:Wait() --PillarClone:Destroy()
					end)
				end 
			end
			if starfolder.BeamSpam.Value == true then task.wait(math.random(33,66)/1000) else task.wait(math.random(20,40)/100) end
		until not workspace:FindFirstChild("RainbowStarSwords") 
	end) coroutine.resume(beamcwrap1)



	The = {" Rainbow Beams "}
	rainbowcwrap1 = coroutine.create(function() repeat RunService.RenderStepped:Wait()
			if RainbowStarSwords and starfolder.RainbowBeams.Value == true then 
				local SpawnPos = (Root.CFrame*CFrame.new(Seed:NextInteger(-75,75),100,Seed:NextInteger(-75,75))).p
				local hit,pos = RayCast(SpawnPos,(SpawnPos-Vector3.new(0,1,0))-SpawnPos,150,{char})
				if hit then

					if starfolder.NoBeamLight.Value == false then RainbowPillarClone1 = RainbowPillar:Clone() else RainbowPillarClone1 = RainbowPillarNoLight:Clone() end
					local RainbowPillarClone = RainbowPillarClone1 RainbowPillarClone.CFrame = CFrame.new(pos+Vector3.new(0,RainbowPillarClone.Size.X/2,0))*CFrame.Angles(0,0,math.rad(90)) --RainbowPillarClone.Parent = starfolder

					colorable = {} table.insert(colorable,RainbowPillarClone) for _,light in pairs(RainbowPillarClone:GetChildren()) do if light:IsA("Light") then table.insert(colorable,light) end end
						--[[spawn(function() 
							for _,Colors in pairs(ColorCycle2) do 
								rTween2 = TweenService:Create(RainbowPillarClone,LightFade2,{Color = Colors}) rTween2:Play() rTween2.Completed:Wait() if not RainbowPillarClone then return end	
							end
						end)]]
					for _,light in pairs(colorable) do 
						rainbowcwrap1tweens = coroutine.create(function() 
							for _,Colors in pairs(ColorCycle2) do 
								rTween2 = TweenService:Create(light,LightFade2,{Color = Colors}) rTween2:Play() rTween2.Completed:Wait() if not RainbowPillarClone then return end	
							end
						end)
						coroutine.resume(rainbowcwrap1tweens)
					end
					RainbowPillarClone.Parent = starfolder --task.wait(.03) task.wait() os.time()
					task.spawn(function() task.wait(.6) LaserSound2 = PillarSound2:Clone() LaserSound2.Parent = RainbowPillarClone
						local PillarTween2 = TweenService:Create(RainbowPillarClone,TweenInfo.new(.45,Enum.EasingStyle.Linear,Enum.EasingDirection.Out,0,false,0),{Transparency = 1,Size = Vector3.new(RainbowPillarClone.Size.X,50,50)})
						local SoundTween = TweenService:Create(LaserSound2,TweenInfo.new(.45,Enum.EasingStyle.Linear,Enum.EasingDirection.Out,0,false,0),{Volume = 0})
						LaserSound2:Play() PillarTween2:Play() SoundTween:Play() --PillarTween2.Completed:Wait() RainbowPillarClone:Remove()
						Debris:AddItem(RainbowPillarClone,0.45)--better for script editor
					end)
				end  
			end
			if starfolder.RainbowBeamSpam.Value == true then task.wait(math.random(33,66)/1000) else task.wait(math.random(15,30)/100) end
		until not workspace:FindFirstChild("RainbowStarSwords") 
	end) coroutine.resume(rainbowcwrap1)

	The = {" Sword1 Rainbow Shards "}
	--if ProllyMobile == true then shardtexture = false end -- it seems like mobile version clamps color values, so glowy textures don't work in emulators?
	--if workspace:FindFirstChild("WeatherMachine") then shardtexture = false else shardtexture = true end 
	rainbowcwrap2 = coroutine.create(function()
		repeat RunService.RenderStepped:Wait() ThisIs = "RenderStepped:Wait() is perfect for preventing studio from crashing when you leave the script editor running for 12 hours and accidentally spawn 9,528,643 instances"

			if RainbowStarSwords and starfolder.RainbowShard1.Value == true then 
				local rProj = RainbowShard:Clone() rProj.CFrame = sword.CFrame * CFrame.Angles(math.random(-180,180),math.random(-180,180),math.random(-180,180))
				rProj.Parent = starfolder rProj.Sparkle.PlaybackSpeed = .25 RainbowShard.Sparkle:Play()--Proj:SetNetworkOwner(nil)
				rProj.Velocity = CFrame.new(sword.CFrame.p,sword.CFrame.p+Vector3.new(Seed:NextNumber(-1,1),Seed:NextNumber(1/2,3/2),Seed:NextNumber(-1,1))).lookVector*Seed:NextNumber(70,150)

				if starfolder.CappedGraphics.Value == true then rProj.Material = Enum.Material.Neon
					task.spawn(function() task.wait(.25) Trans = TweenService:Create(rProj,TweenInfo.new(1,Enum.EasingStyle.Linear,Enum.EasingDirection.In,0,false,0),{Transparency = 0}) Trans:Play() end)
					task.spawn(function() for _,Colors in pairs(CandyBar2) do 
							CandyBarTween2 = TweenService:Create(rProj,LightFade3,{Color = Colors}) CandyBarTween2:Play() CandyBarTween2.Completed:Wait() if not rProj then return end	
						end end)
				else
					for _,text in pairs(rProj:GetChildren()) do
						if text:IsA("Texture") then
							task.spawn(function() task.wait(.25) Trans2 = TweenService:Create(text,TweenInfo.new(1,Enum.EasingStyle.Linear,Enum.EasingDirection.In,0,false,0),{Transparency = 0}) Trans2:Play() end)
							task.spawn(function() for _,Colors in pairs(CandyBar) do 
									CandyBarTween = TweenService:Create(text,LightFade3,{Color3 = Colors}) CandyBarTween:Play() CandyBarTween.Completed:Wait() if not rProj then return end	
								end end)
						end
					end
				end
				--[[ThisIs = {" Collision Group Replication1 "} --oh my gah bro! this actually worked, but ts over complicated...
				task.spawn(function() task.wait(.1) rProj.Touched:Connect(function(hit) 
						if hit:IsA("BasePart") and hit:IsDescendantOf(char) then rProj.CanCollide = false return end
						if hit:IsA("BasePart") and not hit:IsDescendantOf(char) and rProj.CanCollide == false and player:DistanceFromCharacter(rProj.Position) > 7 then rProj.CanCollide = true task.wait(.03) rProj.CanCollide = false end
					end)
				end)]]
				ThisIs = {" Collision Group Replication2 "}
				collidables = {} for _,oof in pairs(char:GetChildren()) do if oof:IsA("BasePart") and oof.CanCollide == true then table.insert(collidables,oof) end end
				task.spawn(function() task.wait(.1) rProj.CanCollide = true for _,nahbro in pairs(collidables) do nocol = Instance.new("NoCollisionConstraint",rProj) nocol.Part0 = rProj nocol.Part1 = nahbro end end) 

				task.spawn(function() task.wait(math.random(2,5)) rProj.Anchored=true rProj.Transparency = 1 
					for _,oof in pairs(rProj:GetChildren()) do if oof:IsA("ParticleEmitter") then oof.Enabled = false end if oof:IsA("Texture") then oof.Texture = "" oof.Transparency = 1 end end
					task.wait(3) rProj:Destroy() 
				end)
			end
			if starfolder.ShardSpam.Value == true then task.wait(math.random(15,25)/100) else task.wait(math.random(30,50)/100) end  
		until not workspace:FindFirstChild("RainbowStarSwords") 
	end) coroutine.resume(rainbowcwrap2)


end

function RainbowStarSwordConnect()
	RainbowStarSword1.Equipped:Connect(function() RainbowStarSword() task.wait(.5) RainbowStarSword1.Parent = player.Backpack end)  --RainbowStarSword1.Unequipped:Connect(RainbowStarSword)
end



