			hider1.MouseButton1Click:Connect(function()
				if invv == false then invv = true
					for _,oof in pairs(workspace:GetDescendants()) do if oof.Name == "Handle" and oof:FindFirstChildWhichIsA("WrapLayer")  then oof.Transparency= 1 end end 
					return end
				if invv == true then invv = false
					for _,oof in pairs(workspace:GetDescendants()) do if oof.Name == "Handle" and oof:FindFirstChildWhichIsA("WrapLayer")  then oof.Transparency= 0 end end 
					return end
			end)



			premover = Button:Clone() premover.Parent = gaminglist premover.Text = "remove broken" premover.BackgroundColor3 = Color3.fromRGB(170, 0, 0)
			premover.MouseButton1Click:Connect(function() for _,fpart in pairs(workspace.Structure:GetDescendants()) do if fpart:IsA("BasePart") then --and not fpart:FindFirstChildWhichIsA("Weld") and not fpart:FindFirstChildWhichIsA("Snap") and not fpart:FindFirstChildWhichIsA("Rotate") and not fpart:FindFirstChildWhichIsA("RotateP")  then 
						for _,froze in pairs(fpart:GetConnectedParts(false)) do 
							if not froze.Anchored and not froze:FindFirstChildWhichIsA("Weld") and not froze:FindFirstChildWhichIsA("Snap") and not froze:FindFirstChildWhichIsA("Rotate") and not froze:FindFirstChildWhichIsA("RotateP")  then froze:Remove() end end
					end end end)

