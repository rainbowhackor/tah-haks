		animspeed.MouseWheelForward:Connect(function() s = 1 Animspeed() animspeed.Text = "100%" char.Humanoid.Animator.AnimationPlayed:Connect(function() Animspeed() end) end)
		animspeed.MouseWheelBackward:Connect(function() s = -1 Animspeed() animspeed.Text = "-100%" char.Humanoid.Animator.AnimationPlayed:Connect(function() Animspeed() end) end)

