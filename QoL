	cac += 1 if cac >= 3 then cac = 0 end 
	if cac == 0 then workspace.Camera.CameraSubject = char.Humanoid end
	if cac == 1 then workspace.Camera.CameraSubject = char.UpperTorso end--.CameraPart end
	if cac == 2 then workspace.Camera.CameraSubject = char.Head end
