			ThisIs = {"Sliding guis - Gravity, JumpHeight, WalkSpeed"}  --the old version
			SliderG = Instance.new("Frame",Spaghetti) SliderG.Name = "Gravity" SliderG.Position = UDim2.new(0.05, 0 , 0, row7) SliderG.Size = UDim2.new(0.95,0 , 0,15) SliderG.BackgroundColor3 = Color3.fromRGB(0, 0, 0) SliderG.BackgroundTransparency = .85   SliderG.ZIndex=2 
			SliderJ = Instance.new("Frame",Spaghetti) SliderJ.Name = "Jump" SliderJ.Position = UDim2.new(0.05, 0 , 0, row16) SliderJ.Size = UDim2.new(0.95,0 , 0,15) SliderJ.BackgroundColor3 = Color3.fromRGB(0, 0, 0) SliderJ.BackgroundTransparency = .85   SliderJ.ZIndex=2 
			SliderR = Instance.new("Frame",Spaghetti) SliderR.Name = "Run" SliderR.Position = UDim2.new(0.05, 0 , 0, row15) SliderR.Size = UDim2.new(0.95,0 , 0,15) SliderR.BackgroundColor3 = Color3.fromRGB(0, 0, 0) SliderR.BackgroundTransparency = .85   SliderR.ZIndex=2  
			SliderW = Instance.new("Frame",Spaghetti) SliderW.Name = "Walk" SliderW.Position = UDim2.new(0.05, 0 , 0, row8) SliderW.Size = UDim2.new(0.95,0 , 0,15) SliderW.BackgroundColor3 = Color3.fromRGB(0, 0, 0) SliderW.BackgroundTransparency = .85   SliderW.ZIndex=2  
			colorG = Color3.fromRGB(170, 85, 0)
			colorR = Color3.fromRGB(0, 170, 127)

			sliders =	{SliderG,SliderJ,SliderW,SliderR} 
			for _,Slide in pairs(sliders) do 

				Mouse = game.Players.LocalPlayer:GetMouse()
				local Sliding = Instance.new("Frame",Slide) Sliding.Name = "Sliding" Sliding.Position = UDim2.new(0, 0 , 0, 0) Sliding.Size = UDim2.new(0.1,0 , 0,15) Sliding.BackgroundColor3 = Color3.fromRGB(0, 255, 0) Sliding.BackgroundTransparency = .7   Sliding.ZIndex=2
				--	local MouseButton = Instance.new("TextButton",Slide) MouseButton.Name = "MouseButton" MouseButton.Position = UDim2.new(0.0, 0 , 0.0, 0) MouseButton.Size = UDim2.new(1,0 , 1,0)   MouseButton.BackgroundTransparency = 1   MouseButton.ZIndex=1
				local MouseButton = Button:Clone() MouseButton.Parent = Slide MouseButton.Text=Slide.Name MouseButton.Name = "MouseButton" MouseButton.Size = UDim2.new(1,0 , 1,0) MouseButton.Position = UDim2.new(0.0, 0 , 0.0, 0) MouseButton.BackgroundTransparency = 1 MouseButton.ZIndex=1
				local Max = Instance.new("NumberValue",Slide) Max.Name = "Max" --Max.Value = 100
				if Slide.Name == "Gravity" then Max.Value = 100 end if Slide.Name == "Jump" then Max.Value = 90 end if Slide.Name == "Run" then Max.Value = 250 end if Slide.Name == "Walk" then Max.Value = 32 end
				local Value1 = Instance.new("NumberValue",Slide) Value1.Name = "Value1" Value1.Value = 7.2
				local MouseisOn = Instance.new("BoolValue",Slide) MouseisOn.Value = false MouseisOn.Name = "MouseisOn"
				local MouseisDown = Instance.new("BoolValue",Slide) MouseisDown.Value = false MouseisDown.Name = "MouseisDown"

				local Point = Instance.new("ImageLabel",Slide) Point.Name = "Point" Point.Position = UDim2.new(0.1, 0 , 0.5, 0) Point.Size = UDim2.new(0,12 , 0,12) Point.Image = "rbxassetid://1217158727" Point.ImageColor3 = Color3.fromRGB(0, 255, 0) Point.BackgroundTransparency = 1 Point.ImageTransparency = .7   Point.ZIndex=4 Point.AnchorPoint = Vector2.new(.5,.5)
				local UnClick = Instance.new("BoolValue",Point) UnClick.Value = false UnClick.Name = "Unclick"
				local MouseDown = Instance.new("ImageLabel",Point) MouseDown.Name = "MouseDown" MouseDown.Position = UDim2.new(0.5, 0 , 0.5, 0) MouseDown.Size = UDim2.new(0,0 , 0,0) MouseDown.Image = "rbxassetid://1217158727" MouseDown.ImageColor3 = Color3.fromRGB(0, 255, 0) MouseDown.BackgroundTransparency = 1 MouseDown.ImageTransparency = .7   MouseDown.ZIndex=4 MouseDown.AnchorPoint = Vector2.new(.5,.5)
				local MouseOn = Instance.new("ImageLabel",Point) MouseOn.Name = "MouseOn" MouseOn.Position = UDim2.new(0.5, 0 , 0.5, 0) MouseOn.Size = UDim2.new(0,0 , 0,0) MouseOn.Image = "rbxassetid://1217158727" MouseOn.ImageColor3 = Color3.fromRGB(0, 255, 0) MouseOn.BackgroundTransparency = 1 MouseOn.ImageTransparency = .7   MouseOn.ZIndex=4 MouseOn.AnchorPoint = Vector2.new(.5,.5)
				local ValueLableP = Instance.new("ImageLabel",Point) ValueLableP.Name = "ValueLabel" ValueLableP.Position = UDim2.new(0.5, 0 , 0, -5) ValueLableP.Size = UDim2.new(0,0 , 0,0) ValueLableP.Image = "http://www.roblox.com/asset/?id=3961048262" ValueLableP.ImageColor3 = Color3.fromRGB(0, 255, 0) ValueLableP.BackgroundTransparency = 1  ValueLableP.ImageTransparency = .5  ValueLableP.ZIndex=4 ValueLableP.AnchorPoint = Vector2.new(.5,1)
