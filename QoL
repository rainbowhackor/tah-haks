colcor = nil
function CharSettings() humy = char:WaitForChild("Humanoid") humy.UseJumpPower = false game.StarterGui:SetCore("SendNotification", {Title="UseJumpPower = false"; Text= "JumpHeight better for gravity" ; Duration=5;})
	workspace.Camera.CameraType = Enum.CameraType.Custom workspace.CurrentCamera.CameraType = Enum.CameraType.Custom
	humy:GetPropertyChangedSignal("Sit"):Connect(function() wait() if humy.Sit == true then GetUp() end end)
	humy:GetPropertyChangedSignal("PlatformStand"):Connect(function() wait() if humy.PlatformStand == true then GetUp() end end)
	if char:FindFirstChild("Meshes/Chocolate Donut Rainbow LimitedAccessory") then camm = -1.25  ME=true an=169402 AnimSet()task.wait(1) an=0 runan=1
		if not game.Lighting:FindFirstChildWhichIsA("ColorCorrectionEffect") then colcor=Instance.new("ColorCorrectionEffect",game.Lighting) colcor.Name = "morecolor" game.Lighting:FindFirstChildWhichIsA("ColorCorrectionEffect").Saturation = 0.4 end
	end

	--if char:FindFirstChild[["Accessory (HappyFace)"]] then char:FindFirstChild[["Accessory (HappyFace)"]]:Remove() end
	--	if char:FindFirstChild[["Accessory (beast)"]] then  end
	for _,target in pairs(char:GetDescendants()) do
		if target:IsA("MeshPart") then 
			if target.MeshId == "rbxassetid://16931221073" then target.Parent:Remove() end
			if target.MeshId == "rbxassetid://17899996187" then target.Transparency = 1 target.HandleWrapLayer.Enabled = false end
		end
	end

end
