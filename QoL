rainbowsparkleTween = nil  periTween = nil  BlindingTween = nil rTween = nil
function rainbowsparkle1()
	if char.UpperTorso:FindFirstChild("RainbowSparkles") then char.UpperTorso.RainbowSparkles:Remove() return end
	task.spawn(function()
		local ColorChanging = Instance.new("Sparkles",char.UpperTorso) ColorChanging.Name = "RainbowSparkles"
		while ColorChanging do
			for _,Colors in pairs(ColorCycle) do 
				rainbowsparkleTween = TweenService:Create(ColorChanging,LightFade,{SparkleColor = Colors}) rainbowsparkleTween:Play()	 rainbowsparkleTween.Completed:Wait() if not ColorChanging then  return  end	
			end
		end
	end)
end
function rainbowsparkle2()
	if char.UpperTorso:FindFirstChild("RainbowSparkles") then char.UpperTorso.RainbowSparkles:Remove() return end
	task.spawn(function()
		local ColorChanging = Instance.new("Sparkles",char.UpperTorso) ColorChanging.Name = "RainbowSparkles"
		while ColorChanging do
			for _,Colors in pairs(periColorCycle) do 
				periTween = TweenService:Create(ColorChanging,periLightFade,{SparkleColor = Colors}) periTween:Play()	 periTween.Completed:Wait() if not ColorChanging then  return  end	
			end
		end
	end)
end
function ColorBlind() 
	if player.PlayerGui.MAMALUIGI:FindFirstChild("ColorBlind") then player.PlayerGui.MAMALUIGI.ColorBlind:Remove() return end
	task.spawn(function() local fui = Instance.new("ScreenGui", player.PlayerGui.MAMALUIGI) fui.Name = "ColorBlind"
		local ColorBlinding = Instance.new("Frame",fui) ColorBlinding.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
		ColorBlinding.Position = UDim2.new(0, 0,-.1,0 ) ColorBlinding.Size = UDim2.new(1, 0 , 1.1, 0) ColorBlinding.BackgroundTransparency = .9  
		while ColorBlinding do
			for _,Colors in pairs(ColorBlindCycle) do 
				BlindingTween = TweenService:Create(ColorBlinding,ColorBlindFade,{BackgroundColor3 = Colors}) BlindingTween:Play()	 BlindingTween.Completed:Wait() if not ColorBlinding then  return  end	
			end
		end
	end)
end

if Ruse == false then
	The = {" Rainbow Color Changing "} if player:FindFirstChild("nooOOOO!!!") then player["nooOOOO!!!"]:Remove() end ColorChanging = Instance.new("Color3Value",player) ColorChanging.Name = "nooOOOO!!!"
	colorcwrap = coroutine.create(function() while ColorChanging do 
			for _,Colors in pairs(ColorCycle) do rTween = TweenService:Create(ColorChanging,LightFade,{Value = Colors}) rTween:Play() rTween.Completed:Wait()
				if not ColorChanging then if rTween then rTween:Pause() rTween:Remove() end return end 
			end end 
	end) coroutine.resume(colorcwrap)
end

CappedGraphics = nil IvoryShard1 = nil  IvoryShard2 = nil  RainbowShard1 = nil Beams = nil RainbowBeams = nil  BeamSpam = nil  ShardSpam = nil 
