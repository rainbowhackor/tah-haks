					obj2 = Instance.new("BoolValue",eha) obj2.Name = "invhat" obj2.Value = false
				end 
			end 
			for _, Buttons in pairs(accesslist:GetChildren()) do --invhat = false storehat = false 
				Buttons.MouseButton1Click:Connect(function() 
					Buttons.invhat.Value = not Buttons.invhat.Value if Buttons.invhat.Value == true then 
						Buttons.Part.Value.Transparency = 1 Buttons.BackgroundColor3 = Color3.fromRGB(0, 0, 0) if Buttons.Part.Value:FindFirstChildWhichIsA("WrapLayer") then Buttons.Part.Value:FindFirstChildWhichIsA("WrapLayer").Enabled = false end
						--if workspace:FindFirstChild("WeatherMachine") then Buttons.Part.Value.Transparency = .97 Buttons.BackgroundColor3 = Color3.fromRGB(0, 0, 0) if Buttons.Part.Value:FindFirstChildWhichIsA("WrapLayer") then Buttons.Part.Value:FindFirstChildWhichIsA("WrapLayer").Enabled = false end end
					else Buttons.Part.Value.Transparency = 0 Buttons.BackgroundColor3 = Color3.fromRGB(0, 127, 0) if Buttons.Part.Value:FindFirstChildWhichIsA("WrapLayer") then Buttons.Part.Value:FindFirstChildWhichIsA("WrapLayer").Enabled = true end end
					--if Buttons.Part.Value.Transparency == 1 then Buttons.Part.Value.Transparency = 0 Buttons.BackgroundColor3 = Color3.fromRGB(0, 127, 0) return end
					--if Buttons.Part.Value.Transparency == 0 then Buttons.Part.Value.Transparency = 1 Buttons.BackgroundColor3 = Color3.fromRGB(127, 0, 0) end
				end)
				Buttons.MouseButton2Click:Connect(function() --Buttons.Value.Value.Parent:Remove()
