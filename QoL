function NewCamera() --char.Humanoid.CameraOffset = Vector3.new(0, camm, 0)
	if Ruse == false then char.Humanoid.CameraOffset = Vector3.new(0, _G.camm, 0) else
		--[[if not char.UpperTorso:FindFirstChild("CameraPart") then CameraPart = Instance.new("Part", char.UpperTorso) CameraPart.Name = "CameraPart" CameraPart.Size = Vector3.new(0,0,0) CameraPart.CanCollide = false CameraPart.CanTouch = false CameraPart.CanQuery = false CameraPart.CastShadow = false CameraPart.Massless = true end
		if not char.UpperTorso.CameraPart:FindFirstChild("CameraWeld") then CameraWeld = Instance.new("Weld", CameraPart) CameraWeld.Name = "CameraWeld" CameraWeld.Part0 = char.UpperTorso CameraWeld.Part1 = CameraPart end
		workspace.Camera.CameraSubject = char.UpperTorso.CameraPart char.UpperTorso.CameraPart.CameraWeld.C0 = CFrame.new(0,camm+1.5,0)]]
		char.Humanoid.CameraOffset = Vector3.new(0, _G.camm, 0)
	end
end
