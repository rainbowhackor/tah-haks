function SEAT() if workspace:FindFirstChild("seater") then workspace:FindFirstChild("seater"):Remove() return end 
	SeatPart = Instance.new("Seat",workspace) SeatPart.CFrame = char.HumanoidRootPart.CFrame SeatPart.Name = "seater"
	SeatPart:Sit(char.Humanoid)
	SeatWeld = Instance.new("Weld", SeatPart) SeatWeld.Archivable = false SeatWeld.Name = "SeatWeld" SeatWeld.Part0 = SeatPart SeatWeld.Part1 = char.HumanoidRootPart 
	char.Humanoid.Sit = true --char.Humanoid:ChangeState("Seated")
	SeatWeld.C0 = CFrame.new(0, 0.5, 0) *CFrame.Angles(math.rad(-90),0,0) SeatWeld.C1 = CFrame.new(0, -1.468, 0) * CFrame.Angles(math.rad(-90),0,0) 
end 
