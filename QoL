	end
end]]

isSwimming = false climbing = false
function GetUp() isSwimming=false char.Humanoid:SetStateEnabled("GettingUp", true) end
function Swim() isSwimming = not isSwimming if isSwimming == true then char.Humanoid:SetStateEnabled("GettingUp", false) char.Humanoid:ChangeState("Swimming") else char.Humanoid:SetStateEnabled("GettingUp", true) end end
function climb() climbing = not climbing if climbing == true then 
		char.Humanoid:SetStateEnabled("Climbing", false) char.Humanoid:SetStateEnabled("FallingDown", false)  else --char.Humanoid:SetStateEnabled("Freefall", false) 
		char.Humanoid:SetStateEnabled("Climbing", true) char.Humanoid:SetStateEnabled("FallingDown", true)  end end --char.Humanoid:SetStateEnabled("Freefall", true)

stand = nil 
function AirBlock() 
	if game.Workspace:FindFirstChild("standin") then game.Workspace:FindFirstChild("standin"):Remove() return end
	stand = Instance.new("Part",workspace) stand.Anchored = true stand.Name = "standin" stand.Size = Vector3.new(3,1,3) stand.Transparency = .85 
	if char:FindFirstChild("LeftFoot") then stand.Position = char.LeftFoot.Position - Vector3.new(0,1,0) end 
	if char.Humanoid.Sit == true then stand.Position = char.HumanoidRootPart.Position - Vector3.new(0,1,0) end
	if char.Humanoid.PlatformStand == true then stand.Position = char.HumanoidRootPart.Position - Vector3.new(0,1,0) end 
end

function REMOVE_LEGS()-- i feel like this only works with higher level executors, or roblox patched this
	for _,v in pairs(char:GetChildren()) do
		if v.Name == "RightUpperLeg" or v.Name == "LeftUpperLeg" or v.Name == "RightLowerLeg" or v.Name == "LeftLowerLeg" or v.Name == "RightFoot" or v.Name == "LeftFoot" or v.Name == "RightLeg" or v.Name == "LeftLeg" then
			v:Destroy()
		end
	end
end
seeleg = false
function HIDE_LEGS() 
	seeleg = not seeleg 
	for _,v in pairs(char:GetChildren()) do
		if v.Name == "RightUpperLeg" or v.Name == "LeftUpperLeg" or v.Name == "RightLowerLeg" or v.Name == "LeftLowerLeg" or v.Name == "RightFoot" or v.Name == "LeftFoot" or v.Name == "RightLeg" or v.Name == "LeftLeg" then
			if seeleg == false then v.Transparency = 1 else v.Transparency = 0 end   
		end
	end
end

Gu = nil epic = nil katana = nil Katana = nil backpack = nil meme = nil
function ShowControls()
	if player.PlayerGui:FindFirstChild("Controls") then player.PlayerGui:FindFirstChild("Controls"):Remove() return end
	Gu = Instance.new("ScreenGui",player.PlayerGui) Gu.Name = "Controls" Gu.Enabled = true
	Button = Instance.new("TextButton",Gu) Button.Name = "controls"  Button.Position = UDim2.new(0, 20 , 0.361, 0) Button.Size = UDim2.new(0,  49 , 0, 360) Button.TextColor3 = Color3.fromRGB(255, 255, 255) Button.BackgroundColor3 = Color3.fromRGB(0, 0, 0) Button.BackgroundTransparency = 1  
	Button.TextXAlignment = Enum.TextXAlignment.Left Button.TextWrapped = false Button.TextScaled = false Button.TextSize = 14 Button.Active = false Button.Active = false Button.Font = Enum.Font.FredokaOne   
	Button.Text = controls
end


function Gears() backpack = player:WaitForChild("Backpack")-- repeat wait() until backpack--or player:WaitForChild("Backpack") -- no one can see these, it's just mental assurance  
	epic = Instance.new("Tool",backpack) epic.Name = "Epic Katana Client" epic.TextureId = "http://www.roblox.com/asset/?id=25265469" epic.Grip = CFrame.new(0, 0, -1.7) * CFrame.Angles(0, math.rad(90), math.rad(90)) epic.RequiresHandle = false
	katana = Instance.new("Part",epic) katana.Name = "Handle" katana.Size = Vector3.new(1,.8,5)  
	meme = Instance.new("SpecialMesh",katana) meme.Scale = Vector3.new(1.7, 1.7, 1.7) meme.VertexColor = Vector3.new(0, 0, 0) meme.TextureId="http://www.roblox.com/asset/?id=11442524"  meme.MeshId = "http://www.roblox.com/asset/?id=11442510"
	local kSparkles1 = Instance.new("Sparkles",katana) kSparkles1.SparkleColor = Color3.fromRGB(-765, -1020, 1530) kSparkles1.Name = "Sparkles1" 
	local kSparkles2 = Instance.new("Sparkles",katana) kSparkles2.SparkleColor = Color3.fromRGB(1020, -255, 1275) kSparkles2.Name = "Sparkles2"
	local kLight1 = Instance.new("SpotLight",katana) kLight1.Color = kSparkles1.Color kLight1.Angle = 180 kLight1.Range = 4 kLight1.Name = "kLight1" kLight1.Face = Enum.NormalId.Top
	local kLight2 = Instance.new("SpotLight",katana) kLight2.Color = kSparkles2.Color kLight2.Angle = 180 kLight2.Range = 4 kLight2.Name = "kLight2" kLight2.Face = Enum.NormalId.Bottom
	Katana = coroutine.create(function() repeat  task.wait(3 ) 
			kSparkles1.SparkleColor = Color3.new(math.random(0, 5), math.random(0, 12), math.random(0, 15)) kLight1.Color = kSparkles1.SparkleColor
			kSparkles2.SparkleColor = Color3.new(math.random(0, 12), math.random(0, 3), math.random(0, 15)) kLight2.Color = kSparkles2.SparkleColor
		until not kSparkles1  end)  coroutine.resume(Katana)

	RainbowStarSword1 = Instance.new("Tool",backpack) RainbowStarSword1.Name = "Rainbow Star Sword Client" RainbowStarSword1.TextureId = "rbxassetid://6854488139" RainbowStarSword1.RequiresHandle = false
	--RainbowStarSword1.Equipped:Connect(RainbowStarSword) RainbowStarSword1.Unequipped:Connect(RainbowStarSword) 
	RainbowStarSword1.Equipped:Connect(NoToolHold)
	epic.Equipped:Connect(NoToolHold) 
	RainbowStarSwordConnect()
	--end)

end	 
